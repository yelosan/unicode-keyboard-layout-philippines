  partial alphanumeric_keys
  xkb_symbols "capewell-qwerf2006" {
    name[Group1] = "Hanunoo (Philippines, Baybayin, Capewell-QWERF2006)";
    key.type[Group1] = "TWO_LEVEL";

    include "ph-hano(capewell-qwerf2007)"

    // TOP row
    key <AD08> { [      U1723, VoidSymbol ] }; // (K) ᜣ (LETTER KA)
    key <AD09> { [      U172E, VoidSymbol ] }; // (L) ᜮ (LETTER LA)

    // HOME row
