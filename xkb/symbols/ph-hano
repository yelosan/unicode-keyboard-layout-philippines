// END: Baybayin-Hanunoo Unicode Keyboard Layout - Capewell-QWERF2006

// BGN: Baybayin-Hanunoo Unicode Keyboard Layout - Dvorak
  // ISO tag for Baybayin-Hanunoo script:
  //   * Hano (U+1720–U+173F) - https://www.unicode.org/charts/PDF/U1720.pdf)
  //
  // Code license: MIT No Attribution (MIT-0)
  // Layout license: CC0 1.0 Universal (Public Domain)
  //   * First released: 2023
  // Repo: https://codeberg.org/yelosan/unicode-keyboard-layout-philippines
  //
  // By: JC John Sese Cuneta (Yelosan Publishing)
  //   * Email: contact@im.youronly.one | jcjohn.sesecuneta@gmail.com
  //   * GPG: https://youronly.one/?verify=keyoxide
  //   * Keybase: https://keybase.io/yukino
  // For: Ubuntu Philippines LoCo Team and the Filipino people
  //
  // Lang: fil, akl, bcl, bik, bnc, cbk, ceb, hil, ibg, ilo, ivb, ivv, krj,
  //       mdh, mlz, mrw, pag, pam, tgl, tgn, tsg, sgd, war, xsb, yka, phi
  //
  partial alphanumeric_keys
  xkb_symbols "dvorak" {
    name[Group1] = "Hanunoo (Philippines, Baybayin, Dvorak)";
    key.type[Group1] = "TWO_LEVEL";

    // NUMERIC row
    include "ph-hano(numeric-row)"

    // NUMERIC ROW
    key <AE11> { [ VoidSymbol, VoidSymbol ] };
    key <AE12> { [ VoidSymbol, VoidSymbol ] };

    // TOP row
    key <AD01> { [ VoidSymbol, VoidSymbol ] }; // (')
    key <AD02> { [      U1735, VoidSymbol ] }; // (,) ᜵ (PHILIPPINE SINGLE PUNCTUATION)
    key <AD03> { [      U1736, VoidSymbol ] }; // (.) ᜶ (PHILIPPINE DOUBLE PUNCTUATION)
    key <AD04> { [      U1729, VoidSymbol ] }; // (P) ᜩ (LETTER PA)
    key <AD05> { [      U172C, VoidSymbol ] }; // (Y) ᜬ (LETTER YA)
    key <AD06> { [ VoidSymbol, VoidSymbol ] }; // (F)
    key <AD07> { [      U1724, VoidSymbol ] }; // (G) ᜤ (LETTER GA)
    key <AD08> { [ VoidSymbol, VoidSymbol ] }; // (C)
    key <AD09> { [      U172D, VoidSymbol ] }; // (R) ᜭ (LETTER RA)
    key <AD10> { [      U172E, VoidSymbol ] }; // (L) ᜮ (LETTER LA)
    key <AD11> { [      U1734, VoidSymbol ] }; // (/) ᜴ (MANGYAN PAMUDPOD)
    key <AD12> { [      U1714, VoidSymbol ] }; // (+) ᜔ (VIRAMA)
    key <BKSL> { [ VoidSymbol, VoidSymbol ] }; // (\)

    // HOME row
    key <AC01> { [      U1720, VoidSymbol ] }; // (A) ᜠ (LETTER A)
    key <AC02> { [      U1733,      U1722 ] }; // (O) ᜳ (VOWEL O) ᜢ (LETTER O)
    key <AC03> { [      U1732,      U1721 ] }; // (E) ᜲ(VOWEL E) ᜡ (LETTER E)
    key <AC04> { [      U1733,      U1722 ] }; // (U) ᜳ (VOWEL U) ᜢ (LETTER U)
    key <AC05> { [      U1732,      U1721 ] }; // (I) ᜲ (VOWEL I) ᜡ (LETTER I)
    key <AC06> { [      U1727, VoidSymbol ] }; // (D) ᜧ (LETTER DA)
    key <AC07> { [      U1731, VoidSymbol ] }; // (H) ᜱ (LETTER HA)
    key <AC08> { [      U1726, VoidSymbol ] }; // (T) ᜦ (LETTER TA)
    key <AC09> { [      U1728,      U1725 ] }; // (N) ᜨ (LETTER NA) ᜥ (LETTER NGA)
    key <AC10> { [      U1730, VoidSymbol ] }; // (S) ᜰ (LETTER SA)
    key <AC11> { [      U1734, VoidSymbol ] }; // (-) ᜴ (MANGYAN PAMUDPOD)
    // AC12 is in some physical keyboards (e.g. OLPC); usually an alias of <BKSL>
    //key <AC12> { [ VoidSymbol, VoidSymbol ] }; // ()

    // BOTTOM row
    // AB00 is in some physical keyboards (e.g. ISO-105)
    //key <AB00> { [ VoidSymbol, VoidSymbol ] }; // ()
    key <AB01> { [ VoidSymbol, VoidSymbol ] }; // (;)
    key <AB02> { [ VoidSymbol, VoidSymbol ] }; // (Q)
    key <AB03> { [ VoidSymbol, VoidSymbol ] }; // (J)
    key <AB04> { [      U1723, VoidSymbol ] }; // (K) ᜣ (LETTER KA)
    key <AB05> { [ VoidSymbol, VoidSymbol ] }; // (X)
    key <AB06> { [      U172A, VoidSymbol ] }; // (B) ᜪ (LETTER BA)
    key <AB07> { [      U172B, VoidSymbol ] }; // (M) ᜫ (LETTER MA)
    key <AB08> { [      U172F, VoidSymbol ] }; // (W) ᜯ (LETTER WA)
    key <AB09> { [ VoidSymbol, VoidSymbol ] }; // (V)
    key <AB10> { [ VoidSymbol, VoidSymbol ] }; // (Z)
    // AB11 is in some physical keyboards (e.g. Japanese)
    //key <AB11> { [ VoidSymbol, VoidSymbol ] }; // ()

    // Not needed for this layout since all related locations moved.
    //include "ph-hano(punctuations)"
  };
// END: Baybayin-Hanunoo Unicode Keyboard Layout - Dvorak

// BGN: Baybayin-Hanunoo Unicode Keyboard Layout - Capewell-Dvorak / C-Dvorak
  // ISO tag for Baybayin-Hanunoo script:
  //   * Hano (U+1720–U+173F) - https://www.unicode.org/charts/PDF/U1720.pdf)
  //
  // Code license: MIT No Attribution (MIT-0)
  // Layout license: CC0 1.0 Universal (Public Domain)
  //   * First released: 2023
  // Repo: https://codeberg.org/yelosan/unicode-keyboard-layout-philippines
  //
  // By: JC John Sese Cuneta (Yelosan Publishing)
  //   * Email: contact@im.youronly.one | jcjohn.sesecuneta@gmail.com
  //   * GPG: https://youronly.one/?verify=keyoxide
  //   * Keybase: https://keybase.io/yukino
  // For: Ubuntu Philippines LoCo Team and the Filipino people
  //
  // Lang: fil, akl, bcl, bik, bnc, cbk, ceb, hil, ibg, ilo, ivb, ivv, krj,
  //       mdh, mlz, mrw, pag, pam, tgl, tgn, tsg, sgd, war, xsb, yka, phi
  //
  partial alphanumeric_keys
  xkb_symbols "capewell-dvorak" {
    name[Group1] = "Hanunoo (Philippines, Baybayin, Capewell-Dvorak)";
    key.type[Group1] = "TWO_LEVEL";

    include "ph-hano(dvorak)"

    // TOP row
    key <AD07> { [ VoidSymbol, VoidSymbol ] }; // (F) override 'include'
    key <AD08> { [      U1724, VoidSymbol ] }; // (G) ᜤ (LETTER GA)
    key <AD10> { [      U1723, VoidSymbol ] }; // (K) ᜣ (LETTER KA)

    // HOME row
    key <AC01> { [      U1733,      U1722 ] }; // (O) ᜳ (VOWEL O) ᜢ (LETTER O)
    key <AC02> { [      U1720, VoidSymbol ] }; // (A) ᜠ (LETTER A)
    key <AC04> { [      U1732,      U1721 ] }; // (I) ᜲ (VOWEL I) ᜡ (LETTER I)
    key <AC05> { [      U1733,      U1722 ] }; // (U) ᜳ (VOWEL U) ᜢ (LETTER U)

    // BOTTOM row
    key <AB04> { [ VoidSymbol, VoidSymbol ] }; // (V) override 'include'
    key <AB06> { [      U172E, VoidSymbol ] }; // (L) ᜮ (LETTER LA)
    key <AB09> { [      U172A, VoidSymbol ] }; // (B) ᜪ (LETTER BA)
  };
// END: Baybayin-Hanunoo Unicode Keyboard Layout - Capewell-Dvorak / C-Dvorak


// BGN: NUMERIC row
  //
  // Included in:
  //   1. QWERTY
  //   2. Colemak
  //   3. Colemak-DH
  //   4. Capewell-QWERF2007
  //   5. Capewell-QWERF2006
  //   6. Dvorak
  //   7. Capewell-Dvorak
  //
  partial alphanumeric_keys
  xkb_symbols "numeric-row" {
    key.type[Group1] = "TWO_LEVEL"; // inherited

    // NUMERIC row
    // TLDE is AE00 in some physical keyboards (e.g. OLPC)
    key <TLDE> { [ VoidSymbol, VoidSymbol ] };
    key <AE01> { [ VoidSymbol, VoidSymbol ] };
    key <AE02> { [ VoidSymbol, VoidSymbol ] };
    key <AE03> { [ VoidSymbol, VoidSymbol ] };
    key <AE04> { [ VoidSymbol, VoidSymbol ] };
    key <AE05> { [ VoidSymbol, VoidSymbol ] };
    key <AE06> { [ VoidSymbol, VoidSymbol ] };
    key <AE07> { [ VoidSymbol, VoidSymbol ] };
    key <AE08> { [ VoidSymbol, VoidSymbol ] };
    key <AE09> { [ VoidSymbol, VoidSymbol ] };
    key <AE10> { [ VoidSymbol, VoidSymbol ] };
    // AE13 is in some physical keyboards (e.g. Japanese)
    key <AE13> { [ VoidSymbol, VoidSymbol ] };
  };
// END: NUMERIC row

// BGN: PUNCTUATIONS
  //
  // Included in:
  //   1. QWERTY
  //   2. Colemak
  //   3. Colemak-DH
  //   4. Capewell-QWERF2007
  //   5. Capewell-QWERF2006
  //
  partial alphanumeric_keys
  xkb_symbols "punctuations" {
    key.type[Group1] = "TWO_LEVEL"; // inherited

    // NUMERIC row
    key <AE11> { [      U1734, VoidSymbol ] }; // (-) ᜴ (MANGYAN PAMUDPOD)
    key <AE12> { [      U1714, VoidSymbol ] }; // (+) ᜔ (VIRAMA)

    // BOTTOM row
    key <AB08> { [      U1735, VoidSymbol ] }; // (,) ᜵ (PHILIPPINE SINGLE PUNCTUATION)
    key <AB09> { [      U1736, VoidSymbol ] }; // (.) ᜶ (PHILIPPINE DOUBLE PUNCTUATION)
    key <AB10> { [      U1734, VoidSymbol ] }; // (/) ᜴ (MANGYAN PAMUDPOD)
  };
// END: PUNCTUATIONS
