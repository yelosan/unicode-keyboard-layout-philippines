// BGN: Baybayin-Hanunoo Unicode Keyboard Layout - QWERTY
  // ISO tag for Baybayin-Hanunoo script:
  //   * Hano (U+1720–U+173F) - https://www.unicode.org/charts/PDF/U1720.pdf)
  //
  // Code license: MIT No Attribution (MIT-0)
  // Layout license: CC0 1.0 Universal (Public Domain)
  //   * First released: 2023
  // Repo: https://codeberg.org/yelosan/unicode-keyboard-layout-philippines
  //
  // By: JC John Sese Cuneta (Yelosan Publishing)
  //   * Email: contact@im.youronly.one | jcjohn.sesecuneta@gmail.com
  //   * GPG: https://youronly.one/?verify=keyoxide
  //   * Keybase: https://keybase.io/yukino
  // For: Ubuntu Philippines LoCo Team and the Filipino people
  //
  // Lang: fil, akl, bcl, bik, bnc, cbk, ceb, hil, ibg, ilo, ivb, ivv, krj,
  //       mdh, mlz, mrw, pag, pam, tgl, tgn, tsg, sgd, war, xsb, yka, phi
  //
  default partial alphanumeric_keys
  xkb_symbols "basic" {
    name[Group1] = "Hanunoo (Philippines, Baybayin)";
    key.type[Group1] = "TWO_LEVEL"; // inherited

    // NUMERIC row
    include "ph-hano(numeric-row)"

    // TOP row
    key <AD01> { [ VoidSymbol, VoidSymbol ] }; // (Q)
    key <AD02> { [      U172F, VoidSymbol ] }; // (W) ᜯ (LETTER WA)
    key <AD03> { [      U1732,      U1721 ] }; // (E) ᜲ(VOWEL E) ᜡ (LETTER E)
    key <AD04> { [      U172D, VoidSymbol ] }; // (R) ᜭ (LETTER RA)
    key <AD05> { [      U1726, VoidSymbol ] }; // (T) ᜦ (LETTER TA)
    key <AD06> { [      U172C, VoidSymbol ] }; // (Y) ᜬ (LETTER YA)
    key <AD07> { [      U1733,      U1722 ] }; // (U) ᜳ (VOWEL U) ᜢ (LETTER U)
    key <AD08> { [      U1732,      U1721 ] }; // (I) ᜲ (VOWEL I) ᜡ (LETTER I)
    key <AD09> { [      U1733,      U1722 ] }; // (O) ᜳ (VOWEL O) ᜢ (LETTER O)
    key <AD10> { [      U1729, VoidSymbol ] }; // (P) ᜩ (LETTER PA)
    key <AD11> { [ VoidSymbol, VoidSymbol ] }; // ([)
    key <AD12> { [ VoidSymbol, VoidSymbol ] }; // (])
    key <BKSL> { [ VoidSymbol, VoidSymbol ] }; // (\)

    // HOME row
    key <AC01> { [      U1720, VoidSymbol ] }; // (A) ᜠ (LETTER A)
    key <AC02> { [      U1730, VoidSymbol ] }; // (S) ᜰ (LETTER SA)
    key <AC03> { [      U1727, VoidSymbol ] }; // (D) ᜧ (LETTER DA)
    key <AC04> { [ VoidSymbol, VoidSymbol ] }; // (F)
    key <AC05> { [      U1724, VoidSymbol ] }; // (G) ᜤ (LETTER GA)
    key <AC06> { [      U1731, VoidSymbol ] }; // (H) ᜱ (LETTER HA)
    key <AC07> { [ VoidSymbol, VoidSymbol ] }; // (J)
    key <AC08> { [      U1723, VoidSymbol ] }; // (K) ᜣ (LETTER KA)
    key <AC09> { [      U172E, VoidSymbol ] }; // (L) ᜮ (LETTER LA)
    key <AC10> { [ VoidSymbol, VoidSymbol ] }; // (;)
    key <AC11> { [ VoidSymbol, VoidSymbol ] }; // (')
    // AC12 is in some physical keyboards (e.g. OLPC); usually an alias of <BKSL>
    //key <AC12> { [ VoidSymbol, VoidSymbol ] }; // ()

    // BOTTOM row
    // AB00 is in some physical keyboards (e.g. ISO-105)
    //key <AB00> { [ VoidSymbol, VoidSymbol ] }; // ()
    key <AB01> { [ VoidSymbol, VoidSymbol ] }; // (Z)
    key <AB02> { [ VoidSymbol, VoidSymbol ] }; // (X)
    key <AB03> { [ VoidSymbol, VoidSymbol ] }; // (C)
    key <AB04> { [ VoidSymbol, VoidSymbol ] }; // (V)
    key <AB05> { [      U172A, VoidSymbol ] }; // (B) ᜪ (LETTER BA)
    key <AB06> { [      U1728,      U1725 ] }; // (N) ᜨ (LETTER NA) ᜥ (LETTER NGA)
    key <AB07> { [      U172B, VoidSymbol ] }; // (M) ᜫ (LETTER MA)
    // AB11 is in some physical keyboards (e.g. Japanese)
    //key <AB11> { [ VoidSymbol, VoidSymbol ] }; // ()

    include "ph-hano(punctuations)"
  };
// END: Baybayin-Hanunoo Unicode Keyboard Layout - QWERTY

// BGN: Baybayin-Hanunoo Unicode Keyboard Layout - Colemak
  // ISO tag for Baybayin-Hanunoo script:
  //   * Hano (U+1720–U+173F) - https://www.unicode.org/charts/PDF/U1720.pdf)
  //
  // Code license: MIT No Attribution (MIT-0)
  // Layout license: CC0 1.0 Universal (Public Domain)
  //   * First released: 2023
  // Repo: https://codeberg.org/yelosan/unicode-keyboard-layout-philippines
  //
  // By: JC John Sese Cuneta (Yelosan Publishing)
  //   * Email: contact@im.youronly.one | jcjohn.sesecuneta@gmail.com
  //   * GPG: https://youronly.one/?verify=keyoxide
  //   * Keybase: https://keybase.io/yukino
  // For: Ubuntu Philippines LoCo Team and the Filipino people
  //
  // Lang: fil, akl, bcl, bik, bnc, cbk, ceb, hil, ibg, ilo, ivb, ivv, krj,
  //       mdh, mlz, mrw, pag, pam, tgl, tgn, tsg, sgd, war, xsb, yka, phi
  //
  partial alphanumeric_keys
  xkb_symbols "colemak" {
    name[Group1] = "Hanunoo (Philippines, Baybayin, Colemak)";
    key.type[Group1] = "TWO_LEVEL"; // inherited

    // NUMERIC row
    include "ph-hano(numeric-row)"

    // TOP row
    key <AD01> { [ VoidSymbol, VoidSymbol ] }; // (Q)
    key <AD02> { [      U172F, VoidSymbol ] }; // (W) ᜯ (LETTER WA)
    key <AD03> { [ VoidSymbol, VoidSymbol ] }; // (F)
    key <AD04> { [      U1729, VoidSymbol ] }; // (P) ᜩ (LETTER PA)
    key <AD05> { [      U1724, VoidSymbol ] }; // (G) ᜤ (LETTER GA)
    key <AD06> { [ VoidSymbol, VoidSymbol ] }; // (J)
    key <AD07> { [      U172E, VoidSymbol ] }; // (L) ᜮ (LETTER LA)
    key <AD08> { [      U1733,      U1722 ] }; // (U) ᜳ (VOWEL U) ᜢ (LETTER U)
    key <AD09> { [      U172C, VoidSymbol ] }; // (Y) ᜬ (LETTER YA)
    key <AD10> { [ VoidSymbol, VoidSymbol ] }; // (;)
    key <AD11> { [ VoidSymbol, VoidSymbol ] }; // ([)
    key <AD12> { [ VoidSymbol, VoidSymbol ] }; // (])
    key <BKSL> { [ VoidSymbol, VoidSymbol ] }; // (\)

    // HOME row
    key <AC01> { [      U1720, VoidSymbol ] }; // (A) ᜠ (LETTER A)
    key <AC02> { [      U172D, VoidSymbol ] }; // (R) ᜭ (LETTER RA)
    key <AC03> { [      U1730, VoidSymbol ] }; // (S) ᜰ (LETTER SA)
    key <AC04> { [      U1726, VoidSymbol ] }; // (T) ᜦ (LETTER TA)
    key <AC05> { [      U1727, VoidSymbol ] }; // (D) ᜧ (LETTER DA)
    key <AC06> { [      U1731, VoidSymbol ] }; // (H) ᜱ (LETTER HA)
    key <AC07> { [      U1728,      U1725 ] }; // (N) ᜨ (LETTER NA) ᜥ (LETTER NGA)
    key <AC08> { [      U1732,      U1721 ] }; // (E) ᜲ(VOWEL E) ᜡ (LETTER E)
    key <AC09> { [      U1732,      U1721 ] }; // (I) ᜲ (VOWEL I) ᜡ (LETTER I)
    key <AC10> { [      U1733,      U1722 ] }; // (O) ᜳ (VOWEL O) ᜢ (LETTER O)
    key <AC11> { [ VoidSymbol, VoidSymbol ] }; // (')
    // AC12 is in some physical keyboards (e.g. OLPC); usually an alias of <BKSL>
    //key <AC12> { [ VoidSymbol, VoidSymbol ] }; // ()

    // BOTTOM row
    // AB00 is in some physical keyboards (e.g. ISO-105)
    //key <AB00> { [ VoidSymbol, VoidSymbol ] }; // ()
    key <AB01> { [ VoidSymbol, VoidSymbol ] }; // (Z)
    key <AB02> { [ VoidSymbol, VoidSymbol ] }; // (X)
    key <AB03> { [ VoidSymbol, VoidSymbol ] }; // (C)
    key <AB04> { [ VoidSymbol, VoidSymbol ] }; // (V)
    key <AB05> { [      U172A, VoidSymbol ] }; // (B) ᜪ (LETTER BA)
    key <AB06> { [      U1723, VoidSymbol ] }; // (K) ᜣ (LETTER KA)
    key <AB07> { [      U172B, VoidSymbol ] }; // (M) ᜫ (LETTER MA)
    // AB11 is in some physical keyboards (e.g. Japanese)
    //key <AB11> { [ VoidSymbol, VoidSymbol ] }; // ()

    include "ph-hano(punctuations)"
  };
// END: Baybayin-Hanunoo Unicode Keyboard Layout - Colemak

// BGN: Baybayin-Hanunoo Unicode Keyboard Layout - Colemak-DH / Colemak-DHm
  // ISO tag for Baybayin-Hanunoo script:
  //   * Hano (U+1720–U+173F) - https://www.unicode.org/charts/PDF/U1720.pdf)
  //
  // Code license: MIT No Attribution (MIT-0)
  // Layout license: CC0 1.0 Universal (Public Domain)
  //   * First released: 2023
  // Repo: https://codeberg.org/yelosan/unicode-keyboard-layout-philippines
  //
  // By: JC John Sese Cuneta (Yelosan Publishing)
  //   * Email: contact@im.youronly.one | jcjohn.sesecuneta@gmail.com
  //   * GPG: https://youronly.one/?verify=keyoxide
  //   * Keybase: https://keybase.io/yukino
  // For: Ubuntu Philippines LoCo Team and the Filipino people
  //
  // Lang: fil, akl, bcl, bik, bnc, cbk, ceb, hil, ibg, ilo, ivb, ivv, krj,
  //       mdh, mlz, mrw, pag, pam, tgl, tgn, tsg, sgd, war, xsb, yka, phi
  //
  partial alphanumeric_keys
  xkb_symbols "colemak-dh" {
    name[Group1] = "Hanunoo (Philippines, Baybayin, Colemak-DH)";
    key.type[Group1] = "TWO_LEVEL";

    include "ph-hano(colemak)"

    // TOP row
    key <AD05> { [      U172A, VoidSymbol ] }; // (B) ᜪ (LETTER BA)

    // HOME row
    key <AC05> { [      U1724, VoidSymbol ] }; // (G) ᜤ (LETTER GA)
    key <AC06> { [      U172B, VoidSymbol ] }; // (M) ᜫ (LETTER MA)

    // BOTTOM row
    key <AB03> { [      U1727, VoidSymbol ] }; // (D) ᜧ (LETTER DA)
    key <AB05> { [ VoidSymbol, VoidSymbol ] }; // (Z) override 'include'
    key <AB07> { [      U1731, VoidSymbol ] }; // (H) ᜱ (LETTER HA)
  };
// END: Baybayin-Hanunoo Unicode Keyboard Layout - Colemak-DH / Colemak-DHm

