// BGN: Baybayin-Tagbanwa Unicode Keyboard Layout - QWERTY
  // ISO tag for Baybayin-Tagbanwa script:
  //   * Tagb (U+1760–U+177F) - https://www.unicode.org/charts/PDF/U1760.pdf)
  //
  // Code license: MIT License
  // Layout license: Public Domain (CC0 1.0 Universal by JC John Sese Cuneta)
  //   * First released: 2023
  // Repo: https://codeberg.org/yelosan/unicode-keyboard-layout-philippines
  //
  // By: JC John Sese Cuneta (Yelosan Publishing)
  //   * Email: contact@im.youronly.one | jcjohn.sesecuneta@gmail.com
  //   * GPG: https://youronly.one/?verify=keyoxide
  //   * Keybase: https://keybase.io/yukino
  // For: Ubuntu Philippines LoCo Team and the Filipino people
  //
  // Lang: fil, akl, bcl, bik, bnc, cbk, ceb, hil, ibg, ilo, ivb, ivv, krj,
  //       mdh, mlz, mrw, pag, pam, tgl, tgn, tsg, sgd, war, xsb, yka, phi
  //
  default partial alphanumeric_keys
  xkb_symbols "basic" {
    name[Group1] = "Tagbanwa (Philippines, Baybayin)";
    key.type[Group1] = "TWO_LEVEL"; // inherited

    // NUMERIC row
    include "ph-tagb(numeric-row)"

    // TOP row
    key <AD01> { [ VoidSymbol, VoidSymbol ] }; // (Q)
    key <AD02> { [      U176F, VoidSymbol ] }; // (W) ᝯ (LETTER WA)
    key <AD03> { [      U1772,      U1761 ] }; // (E) ◌ᝲ (VOWEL E) ᝡ (LETTER E)
    key <AD04> { [ VoidSymbol, VoidSymbol ] }; // (R)
    key <AD05> { [      U1766, VoidSymbol ] }; // (T) ᝦ (LETTER TA)
    key <AD06> { [      U176C, VoidSymbol ] }; // (Y) ᝬ (LETTER YA)
    key <AD07> { [      U1773,      U1762 ] }; // (U) ◌ᝳ (VOWEL U) ᝢ (LETTER U)
    key <AD08> { [      U1772,      U1761 ] }; // (I) ◌ᝲ (VOWEL I) ᝡ (LETTER I)
    key <AD09> { [      U1773,      U1762 ] }; // (O) ◌ᝳ (VOWEL O) ᝢ (LETTER O)
    key <AD10> { [      U1769, VoidSymbol ] }; // (P) ᝩ (LETTER PA)
    key <AD11> { [ VoidSymbol, VoidSymbol ] }; // ([)
    key <AD12> { [ VoidSymbol, VoidSymbol ] }; // (])
    key <BKSL> { [ VoidSymbol, VoidSymbol ] }; // (\)

    // HOME row
    key <AC01> { [      U1760, VoidSymbol ] }; // (A) ᝠ (LETTER A)
    key <AC02> { [      U1770, VoidSymbol ] }; // (S) ᝰ (LETTER SA)
    key <AC03> { [      U1767, VoidSymbol ] }; // (D) ᝧ (LETTER DA)
    key <AC04> { [ VoidSymbol, VoidSymbol ] }; // (F)
    key <AC05> { [      U1764, VoidSymbol ] }; // (G) ᝤ (LETTER GA)
    key <AC06> { [ VoidSymbol, VoidSymbol ] }; // (H)
    key <AC07> { [ VoidSymbol, VoidSymbol ] }; // (J)
    key <AC08> { [      U1763, VoidSymbol ] }; // (K) ᝣ (LETTER KA)
    key <AC09> { [      U176E, VoidSymbol ] }; // (L) ᝮ (LETTER LA)
    key <AC10> { [ VoidSymbol, VoidSymbol ] }; // (;)
    key <AC11> { [ VoidSymbol, VoidSymbol ] }; // (')
    // AC12 is in some physical keyboards (e.g. OLPC); usually an alias of <BKSL>
    //key <AC12> { [ VoidSymbol, VoidSymbol ] }; // ()

    // BOTTOM row
    // AB00 is in some physical keyboards (e.g. ISO-105) (e.g. ISO-105)
    //key <AB00> { [ VoidSymbol, VoidSymbol ] }; // ()
    key <AB01> { [ VoidSymbol, VoidSymbol ] }; // (Z)
    key <AB02> { [ VoidSymbol, VoidSymbol ] }; // (X)
    key <AB03> { [ VoidSymbol, VoidSymbol ] }; // (C)
    key <AB04> { [ VoidSymbol, VoidSymbol ] }; // (V)
    key <AB05> { [      U176A, VoidSymbol ] }; // (B) ᝪ (LETTER BA)
    key <AB06> { [      U1768,      U1765 ] }; // (N) ᝨ (LETTER NA) ᝥ (LETTER NGA)
    key <AB07> { [      U176B, VoidSymbol ] }; // (M) ᝫ (LETTER MA)
    // AB11 is in some physical keyboards (e.g. Japanese)
    //key <AB11> { [ VoidSymbol, VoidSymbol ] }; // ()

    include "ph-tagb(punctuations)"
  };
// END: Baybayin-Tagbanwa Unicode Keyboard Layout - QWERTY

// BGN: Baybayin-Tagbanwa Unicode Keyboard Layout - Colemak
  // ISO tag for Baybayin-Tagbanwa script:
  //   * Tagb (U+1760–U+177F) - https://www.unicode.org/charts/PDF/U1760.pdf)
  //
  // Code license: MIT License
  // Layout license: Public Domain (CC0 1.0 Universal by JC John Sese Cuneta)
  //   * First released: 2023
  // Repo: https://codeberg.org/yelosan/unicode-keyboard-layout-philippines
  //
  // By: JC John Sese Cuneta (Yelosan Publishing)
  //   * Email: contact@im.youronly.one | jcjohn.sesecuneta@gmail.com
  //   * GPG: https://youronly.one/?verify=keyoxide
  //   * Keybase: https://keybase.io/yukino
  // For: Ubuntu Philippines LoCo Team and the Filipino people
  //
  // Lang: fil, akl, bcl, bik, bnc, cbk, ceb, hil, ibg, ilo, ivb, ivv, krj,
  //       mdh, mlz, mrw, pag, pam, tgl, tgn, tsg, sgd, war, xsb, yka, phi
  //
  partial alphanumeric_keys
  xkb_symbols "colemak" {
    name[Group1] = "Tagbanwa (Philippines, Baybayin, Colemak)";
    key.type[Group1] = "TWO_LEVEL"; // inherited

    // NUMERIC row
    include "ph-tagb(numeric-row)"

    // TOP row
    key <AD01> { [ VoidSymbol, VoidSymbol ] }; // (Q)
    key <AD02> { [      U176F, VoidSymbol ] }; // (W) ᝯ (LETTER WA)
    key <AD03> { [ VoidSymbol, VoidSymbol ] }; // (F)
    key <AD04> { [      U1769, VoidSymbol ] }; // (P) ᝩ (LETTER PA)
    key <AD05> { [      U1764, VoidSymbol ] }; // (G) ᝤ (LETTER GA)
    key <AD06> { [ VoidSymbol, VoidSymbol ] }; // (J)
    key <AD07> { [      U176E, VoidSymbol ] }; // (L) ᝮ (LETTER LA)
    key <AD08> { [      U1773,      U1762 ] }; // (U) ◌ᝳ (VOWEL U) ᝢ (LETTER U)
    key <AD09> { [      U176C, VoidSymbol ] }; // (Y) ᝬ (LETTER YA)
    key <AD10> { [ VoidSymbol, VoidSymbol ] }; // (;)
    key <AD11> { [ VoidSymbol, VoidSymbol ] }; // ([)
    key <AD12> { [ VoidSymbol, VoidSymbol ] }; // (])
    key <BKSL> { [ VoidSymbol, VoidSymbol ] }; // (\)

    // HOME row
    key <AC01> { [      U1760, VoidSymbol ] }; // (A) ᝠ (LETTER A)
    key <AC02> { [ VoidSymbol, VoidSymbol ] }; // (R)
    key <AC03> { [      U1770, VoidSymbol ] }; // (S) ᝰ (LETTER SA)
    key <AC04> { [      U1766, VoidSymbol ] }; // (T) ᝦ (LETTER TA)
    key <AC05> { [      U1767, VoidSymbol ] }; // (D) ᝧ (LETTER DA)
    key <AC06> { [ VoidSymbol, VoidSymbol ] }; // (H)
    key <AC07> { [      U1768,      U1765 ] }; // (N) ᝨ (LETTER NA) ᝥ (LETTER NGA)
    key <AC08> { [      U1772,      U1761 ] }; // (E) ◌ᝲ (VOWEL E) ᝡ (LETTER E)
    key <AC09> { [      U1772,      U1761 ] }; // (I) ◌ᝲ (VOWEL I) ᝡ (LETTER I)
    key <AC10> { [      U1773,      U1762 ] }; // (O) ◌ᝳ (VOWEL O) ᝢ (LETTER O)
    key <AC11> { [ VoidSymbol, VoidSymbol ] }; // (')
    // AC12 is in some physical keyboards (e.g. OLPC); usually an alias of <BKSL>
    //key <AC12> { [ VoidSymbol, VoidSymbol ] }; // ()

    // BOTTOM row
    // AB00 is in some physical keyboards (e.g. ISO-105) (e.g. ISO-105)
    //key <AB00> { [ VoidSymbol, VoidSymbol ] }; // ()
    key <AB01> { [ VoidSymbol, VoidSymbol ] }; // (Z)
    key <AB02> { [ VoidSymbol, VoidSymbol ] }; // (X)
    key <AB03> { [ VoidSymbol, VoidSymbol ] }; // (C)
    key <AB04> { [ VoidSymbol, VoidSymbol ] }; // (V)
    key <AB05> { [      U176A, VoidSymbol ] }; // (B) ᝪ (LETTER BA)
    key <AB06> { [      U1763, VoidSymbol ] }; // (K) ᝣ (LETTER KA)
    key <AB07> { [      U176B, VoidSymbol ] }; // (M) ᝫ (LETTER MA)
    // AB11 is in some physical keyboards (e.g. Japanese)
    //key <AB11> { [ VoidSymbol, VoidSymbol ] }; // ()

    include "ph-tagb(punctuations)"
  };
// END: Baybayin-Tagbanwa Unicode Keyboard Layout - Colemak

// BGN: Baybayin-Tagbanwa Unicode Keyboard Layout - Colemak-DH / Colemak-DHm
  // ISO tag for Baybayin-Tagbanwa script:
  //   * Tagb (U+1760–U+177F) - https://www.unicode.org/charts/PDF/U1760.pdf)
  //
  // Code license: MIT License
  // Layout license: Public Domain (CC0 1.0 Universal by JC John Sese Cuneta)
  //   * First released: 2023
  // Repo: https://codeberg.org/yelosan/unicode-keyboard-layout-philippines
  //
  // By: JC John Sese Cuneta (Yelosan Publishing)
  //   * Email: contact@im.youronly.one | jcjohn.sesecuneta@gmail.com
  //   * GPG: https://youronly.one/?verify=keyoxide
  //   * Keybase: https://keybase.io/yukino
  // For: Ubuntu Philippines LoCo Team and the Filipino people
  //
  // Lang: fil, akl, bcl, bik, bnc, cbk, ceb, hil, ibg, ilo, ivb, ivv, krj,
  //       mdh, mlz, mrw, pag, pam, tgl, tgn, tsg, sgd, war, xsb, yka, phi
  //
  partial alphanumeric_keys
  xkb_symbols "colemak-dh" {
    name[Group1] = "Tagbanwa (Philippines, Baybayin, Colemak-DH)";
    key.type[Group1] = "TWO_LEVEL";

    include "ph-tagb(colemak)"

    // TOP row
    key <AD05> { [      U176A, VoidSymbol ] }; // (B) ᝪ (LETTER BA)

    // HOME row
    key <AC05> { [      U1764, VoidSymbol ] }; // (G) ᝤ (LETTER GA)
    key <AC06> { [      U176B, VoidSymbol ] }; // (M) ᝫ (LETTER MA)

    // BOTTOM row
    key <AB03> { [      U1767, VoidSymbol ] }; // (D) ᝧ (LETTER DA)
    key <AB05> { [ VoidSymbol, VoidSymbol ] }; // (Z) override 'include'
    key <AB07> { [ VoidSymbol, VoidSymbol ] }; // (H) override 'include'
  };
// END: Baybayin-Tagbanwa Unicode Keyboard Layout - Colemak-DH / Colemak-DHm

