  partial alphanumeric_keys
  xkb_symbols "capewell-qwerf2006" {
    name[Group1] = "Tagbanwa (Philippines, Baybayin, Capewell-QWERF2006)";
    key.type[Group1] = "TWO_LEVEL";

    include "ph-tagb(capewell-qwerf2007)"

    // TOP row
    key <AD08> { [      U1763, VoidSymbol ] }; // (K) ᝣ (LETTER KA)
    key <AD09> { [      U176E, VoidSymbol ] }; // (L) ᝮ (LETTER LA)

    // HOME row
    key <AC08> { [      U1772,      U1761 ] }; // (I) ◌ᝲ (VOWEL I) ᝡ (LETTER I)
    key <AC09> { [      U1773,      U1762 ] }; // (O) ◌ᝳ (VOWEL O) ᝢ (LETTER O)
  };
// END: Baybayin-Tagbanwa Unicode Keyboard Layout - Capewell-QWERF2006

// BGN: Baybayin-Tagbanwa Unicode Keyboard Layout - Dvorak
  // ISO tag for Baybayin-Tagbanwa script:
  //   * Tagb (U+1760–U+177F) - https://www.unicode.org/charts/PDF/U1760.pdf)
  //
  // Code license: MIT License
  // Layout license: Public Domain (CC0 1.0 Universal by JC John Sese Cuneta)
  //   * First released: 2023
  // Repo: https://codeberg.org/yelosan/unicode-keyboard-layout-philippines
  //
  // By: JC John Sese Cuneta (Yelosan Publishing)
  //   * Email: contact@im.youronly.one | jcjohn.sesecuneta@gmail.com
  //   * GPG: https://youronly.one/?verify=keyoxide
  //   * Keybase: https://keybase.io/yukino
  // For: Ubuntu Philippines LoCo Team and the Filipino people
  //
  // Lang: fil, akl, bcl, bik, bnc, cbk, ceb, hil, ibg, ilo, ivb, ivv, krj,
  //       mdh, mlz, mrw, pag, pam, tgl, tgn, tsg, sgd, war, xsb, yka, phi
  //
  partial alphanumeric_keys
  xkb_symbols "dvorak" {
    name[Group1] = "Tagbanwa (Philippines, Baybayin, Dvorak)";
    key.type[Group1] = "TWO_LEVEL";

    // NUMERIC row
    include "ph-tagb(numeric-row)"

    // NUMERIC ROW
    key <AE11> { [ VoidSymbol, VoidSymbol ] };
    key <AE12> { [ VoidSymbol, VoidSymbol ] };

    // TOP row
    key <AD01> { [ VoidSymbol, VoidSymbol ] }; // (')
    key <AD02> { [      U1735, VoidSymbol ] }; // (,) ᜵ (PHILIPPINE SINGLE PUNCTUATION)
    key <AD03> { [      U1736, VoidSymbol ] }; // (.) ᜶ (PHILIPPINE DOUBLE PUNCTUATION)
    key <AD04> { [      U1769, VoidSymbol ] }; // (P) ᝩ (LETTER PA)
    key <AD05> { [      U176C, VoidSymbol ] }; // (Y) ᝬ (LETTER YA)
    key <AD06> { [ VoidSymbol, VoidSymbol ] }; // (F)
    key <AD07> { [      U1764, VoidSymbol ] }; // (G) ᝤ (LETTER GA)
    key <AD08> { [ VoidSymbol, VoidSymbol ] }; // (C)
    key <AD09> { [ VoidSymbol, VoidSymbol ] }; // (R)
    key <AD10> { [      U176E, VoidSymbol ] }; // (L) ᝮ (LETTER LA)
    key <AD11> { [      U1715, VoidSymbol ] }; // (/) ᜕ (TAGALOG PAMUDPOD)
    key <AD12> { [      U1714, VoidSymbol ] }; // (+) ᜔ (VIRAMA)
    key <BKSL> { [ VoidSymbol, VoidSymbol ] }; // (\)

    // HOME row
    key <AC01> { [      U1760, VoidSymbol ] }; // (A) ᝠ (LETTER A)
    key <AC02> { [      U1773,      U1762 ] }; // (O) ◌ᝳ (VOWEL O) ᝢ (LETTER O)
    key <AC03> { [      U1772,      U1761 ] }; // (E) ◌ᝲ (VOWEL E) ᝡ (LETTER E)
    key <AC04> { [      U1773,      U1762 ] }; // (U) ◌ᝳ (VOWEL U) ᝢ (LETTER U)
    key <AC05> { [      U1772,      U1761 ] }; // (I) ◌ᝲ (VOWEL I) ᝡ (LETTER I)
    key <AC06> { [      U1767, VoidSymbol ] }; // (D) ᝧ (LETTER DA)
    key <AC07> { [ VoidSymbol, VoidSymbol ] }; // (H)
    key <AC08> { [      U1766, VoidSymbol ] }; // (T) ᝦ (LETTER TA)
    key <AC09> { [      U1768,      U1765 ] }; // (N) ᝨ (LETTER NA) ᝥ (LETTER NGA)
    key <AC10> { [      U1770, VoidSymbol ] }; // (S) ᝰ (LETTER SA)
    key <AC11> { [      U1715, VoidSymbol ] }; // (-) ᜕ (TAGALOG PAMUDPOD)
    // AC12 is in some physical keyboards (e.g. OLPC); usually an alias of <BKSL>
    //key <AC12> { [ VoidSymbol, VoidSymbol ] }; // ()

    // BOTTOM row
    // AB00 is in some physical keyboards (e.g. ISO-105) (e.g. ISO-105)
    //key <AB00> { [ VoidSymbol, VoidSymbol ] }; // ()
    key <AB01> { [ VoidSymbol, VoidSymbol ] }; // (;)
    key <AB02> { [ VoidSymbol, VoidSymbol ] }; // (Q)
    key <AB03> { [ VoidSymbol, VoidSymbol ] }; // (J)
    key <AB04> { [      U1763, VoidSymbol ] }; // (K) ᝣ (LETTER KA)
    key <AB05> { [ VoidSymbol, VoidSymbol ] }; // (X)
    key <AB06> { [      U176A, VoidSymbol ] }; // (B) ᝪ (LETTER BA)
    key <AB07> { [      U176B, VoidSymbol ] }; // (M) ᝫ (LETTER MA)
    key <AB08> { [      U176F, VoidSymbol ] }; // (W) ᝯ (LETTER WA)
    key <AB09> { [ VoidSymbol, VoidSymbol ] }; // (V)
    key <AB10> { [ VoidSymbol, VoidSymbol ] }; // (Z)
    // AB11 is in some physical keyboards (e.g. Japanese)
    //key <AB11> { [ VoidSymbol, VoidSymbol ] }; // ()

    // Not needed for this layout since all related locations moved.
    //include "ph-tagb(punctuations)"
  };
// END: Baybayin-Tagbanwa Unicode Keyboard Layout - Dvorak

// BGN: Baybayin-Tagbanwa Unicode Keyboard Layout - Capewell-Dvorak / C-Dvorak
  // ISO tag for Baybayin-Tagbanwa script:
  //   * Tagb (U+1760–U+177F) - https://www.unicode.org/charts/PDF/U1760.pdf)
  //
  // Code license: MIT License
  // Layout license: Public Domain (CC0 1.0 Universal by JC John Sese Cuneta)
  //   * First released: 2023
  // Repo: https://codeberg.org/yelosan/unicode-keyboard-layout-philippines
  //
  // By: JC John Sese Cuneta (Yelosan Publishing)
  //   * Email: contact@im.youronly.one | jcjohn.sesecuneta@gmail.com
  //   * GPG: https://youronly.one/?verify=keyoxide
  //   * Keybase: https://keybase.io/yukino
  // For: Ubuntu Philippines LoCo Team and the Filipino people
  //
  // Lang: fil, akl, bcl, bik, bnc, cbk, ceb, hil, ibg, ilo, ivb, ivv, krj,
  //       mdh, mlz, mrw, pag, pam, tgl, tgn, tsg, sgd, war, xsb, yka, phi
  //
  partial alphanumeric_keys
  xkb_symbols "capewell-dvorak" {
    name[Group1] = "Tagbanwa (Philippines, Baybayin, Capewell-Dvorak)";
    key.type[Group1] = "TWO_LEVEL";

    include "ph-tagb(dvorak)"

    // TOP row
    key <AD07> { [ VoidSymbol, VoidSymbol ] }; // (F) override 'include'
    key <AD08> { [      U1764, VoidSymbol ] }; // (G) ᝤ (LETTER GA)
    key <AD10> { [      U1763, VoidSymbol ] }; // (K) ᝣ (LETTER KA)

    // HOME row
    key <AC01> { [      U1773,      U1762 ] }; // (O) ◌ᝳ (VOWEL O) ᝢ (LETTER O)
    key <AC02> { [      U1760, VoidSymbol ] }; // (A) ᝠ (LETTER A)
    key <AC04> { [      U1772,      U1761 ] }; // (I) ◌ᝲ (VOWEL I) ᝡ (LETTER I)
    key <AC05> { [      U1773,      U1762 ] }; // (U) ◌ᝳ (VOWEL U) ᝢ (LETTER U)

    // BOTTOM row
    key <AB04> { [ VoidSymbol, VoidSymbol ] }; // (V) override 'include'
    key <AB06> { [      U176E, VoidSymbol ] }; // (L) ᝮ (LETTER LA)
    key <AB09> { [      U176A, VoidSymbol ] }; // (B) ᝪ (LETTER BA)
  };
// END: Baybayin-Tagbanwa Unicode Keyboard Layout - Capewell-Dvorak / C-Dvorak


// BGN: NUMERIC row
  //
  // Included in:
  //   1. QWERTY
  //   2. Colemak
  //   3. Colemak-DH
  //   4. Capewell-QWERF2007
  //   5. Capewell-QWERF2006
  //   6. Dvorak
  //   7. Capewell-Dvorak
  //
  partial alphanumeric_keys
  xkb_symbols "numeric-row" {
    key.type[Group1] = "TWO_LEVEL"; // inherited

    // NUMERIC row
    // TLDE is AE00 in some physical keyboards (e.g. OLPC)
    key <TLDE> { [ VoidSymbol, VoidSymbol ] };
    key <AE01> { [ VoidSymbol, VoidSymbol ] };
    key <AE02> { [ VoidSymbol, VoidSymbol ] };
    key <AE03> { [ VoidSymbol, VoidSymbol ] };
    key <AE04> { [ VoidSymbol, VoidSymbol ] };
    key <AE05> { [ VoidSymbol, VoidSymbol ] };
    key <AE06> { [ VoidSymbol, VoidSymbol ] };
    key <AE07> { [ VoidSymbol, VoidSymbol ] };
    key <AE08> { [ VoidSymbol, VoidSymbol ] };
    key <AE09> { [ VoidSymbol, VoidSymbol ] };
    key <AE10> { [ VoidSymbol, VoidSymbol ] };
    // AE13 is in some physical keyboards (e.g. Japanese)
    key <AE13> { [ VoidSymbol, VoidSymbol ] };
  };
// END: NUMERIC row

// BGN: PUNCTUATIONS
  //
  // Included in:
  //   1. QWERTY
  //   2. Colemak
  //   3. Colemak-DH
  //   4. Capewell-QWERF2007
  //   5. Capewell-QWERF2006
  //
  partial alphanumeric_keys
  xkb_symbols "punctuations" {
    key.type[Group1] = "TWO_LEVEL"; // inherited

    // NUMERIC row
    key <AE11> { [      U1715, VoidSymbol ] }; // (-) ᜕ (TAGALOG PAMUDPOD)
    key <AE12> { [      U1714, VoidSymbol ] }; // (+) ᜔ (VIRAMA)

    // BOTTOM row
    key <AB08> { [      U1735, VoidSymbol ] }; // (,) ᜵ (PHILIPPINE SINGLE PUNCTUATION)
    key <AB09> { [      U1736, VoidSymbol ] }; // (.) ᜶ (PHILIPPINE DOUBLE PUNCTUATION)
    key <AB10> { [      U1715, VoidSymbol ] }; // (/) ᜕ (TAGALOG PAMUDPOD)
  };
// END: PUNCTUATIONS
