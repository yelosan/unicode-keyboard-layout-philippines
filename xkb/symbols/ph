    key <AC07> { [      U1711, VoidSymbol ] }; // (H) ·úë (LETTER HA)
    key <AC08> { [      U1706, VoidSymbol ] }; // (T) ·úÜ (LETTER TA)
    key <AC09> { [      U1708,      U1705 ] }; // (N) ·úà (LETTER NA) ·úÖ (LETTER NGA)
