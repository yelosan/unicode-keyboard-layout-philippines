    key <AC04> { [      U1766, VoidSymbol ] }; // (T) ·ù¶ (LETTER TA)
    key <AC05> { [      U1767, VoidSymbol ] }; // (D) ·ùß (LETTER DA)
    key <AC06> { [ VoidSymbol, VoidSymbol ] }; // (H)
    key <AC07> { [      U1768,      U1765 ] }; // (N) ·ù® (LETTER NA) ·ù• (LETTER NGA)
    key <AC08> { [      U1772,      U1761 ] }; // (E) ‚óå·ù≤ (VOWEL E) ·ù° (LETTER E)
    key <AC09> { [      U1772,      U1761 ] }; // (I) ‚óå·ù≤ (VOWEL I) ·ù° (LETTER I)
    key <AC10> { [      U1773,      U1762 ] }; // (O) ‚óå·ù≥ (VOWEL O) ·ù¢ (LETTER O)
    key <AC11> { [ VoidSymbol, VoidSymbol ] }; // (')

    // BOTTOM row
    key <AB01> { [ VoidSymbol, VoidSymbol ] }; // (Z)
    key <AB02> { [ VoidSymbol, VoidSymbol ] }; // (X)
    key <AB03> { [ VoidSymbol, VoidSymbol ] }; // (C)
    key <AB04> { [ VoidSymbol, VoidSymbol ] }; // (V)
    key <AB05> { [      U176A, VoidSymbol ] }; // (B) ·ù™ (LETTER BA)
    key <AB06> { [      U1763, VoidSymbol ] }; // (K) ·ù£ (LETTER KA)
    key <AB07> { [      U176B, VoidSymbol ] }; // (M) ·ù´ (LETTER MA)

    include "ph(baybayin-punctuations)"
  };
// END: Baybayin-Tagbanwa Unicode Keyboard Layout - Colemak

// BGN: Baybayin-Tagbanwa Unicode Keyboard Layout - Colemak-DH / Colemak-DHm
  // ISO tag for Baybayin-Tagbanwa script:
  //   * Tagb (U+1760‚ÄìU+177F) - https://www.unicode.org/charts/PDF/U1760.pdf)
