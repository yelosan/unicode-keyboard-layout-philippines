// BGN: Baybayin-Hanunoo Unicode Keyboard Layout - QWERTY
  // ISO tag for Baybayin-Hanunoo script:
  //   * Hano (U+1720‚ÄìU+173F) - https://www.unicode.org/charts/PDF/U1720.pdf)
  //
  // Code license: MIT No Attribution (MIT-0)
  // Layout license: CC0 1.0 Universal (Public Domain)
  //   * First released: 2023
  // Repo: https://codeberg.org/yelosan/unicode-keyboard-layout-philippines
  //
  // By: JC John Sese Cuneta (Yelosan Publishing)
  //   * Email: contact@im.youronly.one | jcjohn.sesecuneta@gmail.com
  // For: Ubuntu Philippines LoCo Team and the Filipino people
  //
  partial alphanumeric_keys
  xkb_symbols "hano-qwerty" {
    name[Group1] = "Hanunoo (Baybayin)";
    key.type[Group1] = "TWO_LEVEL";

    // NUMERIC row
    include "ph(baybayin-numeric-row)"

    // TOP row
    key <AD01> { [ VoidSymbol, VoidSymbol ] }; // (Q)
    key <AD02> { [      U172F, VoidSymbol ] }; // (W) ·úØ (LETTER WA)
    key <AD03> { [      U1732,      U1721 ] }; // (E) ·ú≤(VOWEL E) ·ú° (LETTER E)
    key <AD04> { [      U172D, VoidSymbol ] }; // (R) ·ú≠ (LETTER RA)
    key <AD05> { [      U1726, VoidSymbol ] }; // (T) ·ú¶ (LETTER TA)
    key <AD06> { [      U172C, VoidSymbol ] }; // (Y) ·ú¨ (LETTER YA)
    key <AD07> { [      U1733,      U1722 ] }; // (U) ·ú≥ (VOWEL U) ·ú¢ (LETTER U)
    key <AD08> { [      U1732,      U1721 ] }; // (I) ·ú≤ (VOWEL I) ·ú° (LETTER I)
    key <AD09> { [      U1733,      U1722 ] }; // (O) ·ú≥ (VOWEL O) ·ú¢ (LETTER O)
    key <AD10> { [      U1729, VoidSymbol ] }; // (P) ·ú© (LETTER PA)
    key <AD11> { [ VoidSymbol, VoidSymbol ] }; // ([)
    key <AD12> { [ VoidSymbol, VoidSymbol ] }; // (])
    key <BKSL> { [ VoidSymbol, VoidSymbol ] }; // (\)

    // HOME row
    key <AC01> { [      U1720, VoidSymbol ] }; // (A) ·ú† (LETTER A)
    key <AC02> { [      U1730, VoidSymbol ] }; // (S) ·ú∞ (LETTER SA)
    key <AC03> { [      U1727, VoidSymbol ] }; // (D) ·úß (LETTER DA)
    key <AC04> { [ VoidSymbol, VoidSymbol ] }; // (F)
    key <AC05> { [      U1724, VoidSymbol ] }; // (G) ·ú§ (LETTER GA)
    key <AC06> { [      U1731, VoidSymbol ] }; // (H) ·ú± (LETTER HA)
    key <AC07> { [ VoidSymbol, VoidSymbol ] }; // (J)
    key <AC08> { [      U1723, VoidSymbol ] }; // (K) ·ú£ (LETTER KA)
    key <AC09> { [      U172E, VoidSymbol ] }; // (L) ·úÆ (LETTER LA)
    key <AC10> { [ VoidSymbol, VoidSymbol ] }; // (;)
    key <AC11> { [ VoidSymbol, VoidSymbol ] }; // (')

    // BOTTOM row
    key <AB01> { [ VoidSymbol, VoidSymbol ] }; // (Z)
    key <AB02> { [ VoidSymbol, VoidSymbol ] }; // (X)
    key <AB03> { [ VoidSymbol, VoidSymbol ] }; // (C)
    key <AB04> { [ VoidSymbol, VoidSymbol ] }; // (V)
    key <AB05> { [      U172A, VoidSymbol ] }; // (B) ·ú™ (LETTER BA)
    key <AB06> { [      U1728,      U1725 ] }; // (N) ·ú® (LETTER NA) ·ú• (LETTER NGA)
    key <AB07> { [      U172B, VoidSymbol ] }; // (M) ·ú´ (LETTER MA)

    include "ph(mangyan-punctuations)"
  };
// END: Baybayin-Hanunoo Unicode Keyboard Layout - QWERTY

