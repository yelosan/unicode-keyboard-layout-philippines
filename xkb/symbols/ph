  partial alphanumeric_keys
  xkb_symbols "tagb-dvorak" {
    name[Group1] = "Tagbanwa (Baybayin, Dvorak)";
    key.type[Group1] = "TWO_LEVEL";

    // NUMERIC row
    include "ph(baybayin-numeric-row)"

    // NUMERIC ROW
    key <AE11> { [ VoidSymbol, VoidSymbol ] };
    key <AE12> { [ VoidSymbol, VoidSymbol ] };

    // TOP row
    key <AD01> { [ VoidSymbol, VoidSymbol ] }; // (')
    key <AD02> { [      U1735, VoidSymbol ] }; // (,) ·úµ (PHILIPPINE SINGLE PUNCTUATION)
    key <AD03> { [      U1736, VoidSymbol ] }; // (.) ·ú∂ (PHILIPPINE DOUBLE PUNCTUATION)
    key <AD04> { [      U1769, VoidSymbol ] }; // (P) ·ù© (LETTER PA)
    key <AD05> { [      U176C, VoidSymbol ] }; // (Y) ·ù¨ (LETTER YA)
    key <AD06> { [ VoidSymbol, VoidSymbol ] }; // (F)
    key <AD07> { [      U1764, VoidSymbol ] }; // (G) ·ù§ (LETTER GA)
    key <AD08> { [ VoidSymbol, VoidSymbol ] }; // (C)
    key <AD09> { [ VoidSymbol, VoidSymbol ] }; // (R)
    key <AD10> { [      U176E, VoidSymbol ] }; // (L) ·ùÆ (LETTER LA)
    key <AD11> { [      U1715, VoidSymbol ] }; // (/) ·úï (TAGALOG PAMUDPOD)
    key <AD12> { [      U1714, VoidSymbol ] }; // (+) ·úî (VIRAMA)
    key <BKSL> { [ VoidSymbol, VoidSymbol ] }; // (\)

    // HOME row
    key <AC01> { [      U1760, VoidSymbol ] }; // (A) ·ù† (LETTER A)
    key <AC02> { [      U1773,      U1762 ] }; // (O) ‚óå·ù≥ (VOWEL O) ·ù¢ (LETTER O)
    key <AC03> { [      U1772,      U1761 ] }; // (E) ‚óå·ù≤ (VOWEL E) ·ù° (LETTER E)
    key <AC04> { [      U1773,      U1762 ] }; // (U) ‚óå·ù≥ (VOWEL U) ·ù¢ (LETTER U)
    key <AC05> { [      U1772,      U1761 ] }; // (I) ‚óå·ù≤ (VOWEL I) ·ù° (LETTER I)
    key <AC06> { [      U1767, VoidSymbol ] }; // (D) ·ùß (LETTER DA)
    key <AC07> { [ VoidSymbol, VoidSymbol ] }; // (H)
    key <AC08> { [      U1766, VoidSymbol ] }; // (T) ·ù¶ (LETTER TA)
    key <AC09> { [      U1768,      U1765 ] }; // (N) ·ù® (LETTER NA) ·ù• (LETTER NGA)
    key <AC10> { [      U1770, VoidSymbol ] }; // (S) ·ù∞ (LETTER SA)
    key <AC11> { [      U1715, VoidSymbol ] }; // (-) ·úï (TAGALOG PAMUDPOD)

    // BOTTOM row
    key <AB01> { [ VoidSymbol, VoidSymbol ] }; // (;)
    key <AB02> { [ VoidSymbol, VoidSymbol ] }; // (Q)
    key <AB03> { [ VoidSymbol, VoidSymbol ] }; // (J)
    key <AB04> { [      U1763, VoidSymbol ] }; // (K) ·ù£ (LETTER KA)
    key <AB05> { [ VoidSymbol, VoidSymbol ] }; // (X)
    key <AB06> { [      U176A, VoidSymbol ] }; // (B) ·ù™ (LETTER BA)
    key <AB07> { [      U176B, VoidSymbol ] }; // (M) ·ù´ (LETTER MA)
    key <AB08> { [      U176F, VoidSymbol ] }; // (W) ·ùØ (LETTER WA)
    key <AB09> { [ VoidSymbol, VoidSymbol ] }; // (V)
    key <AB10> { [ VoidSymbol, VoidSymbol ] }; // (Z)
  };
// END: Baybayin-Tagbanwa Unicode Keyboard Layout - Dvorak

