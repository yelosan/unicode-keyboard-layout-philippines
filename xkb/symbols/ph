// BGN: Philippines Unicode Keyboard Layout - QWERTY
  //
  // Code license: MIT
  // Layout license: Creative Commons Attribution (CC BY) 4.0 International
  //   * First released: 2010
  // Repo: https://codeberg.org/yelosan/unicode-keyboard-layout-philippines
  //
  // By: JC John Sese Cuneta (Yelosan Publishing)
  //   * Email: contact@im.youronly.one | jcjohn.sesecuneta@gmail.com
  // For: Ubuntu Philippines LoCo Team and the Filipino people
  //
  // Based on: ISO/IEC 9995-3:2010 as of 2023-02-05
  //   * https://en.wikipedia.org/wiki/ISO/IEC_9995
  //
  default partial alphanumeric_keys
  xkb_symbols "basic" {
    name[Group1] = "Filipino";

    // NUMERIC row
    include "ph(numeric-row)"

    // TOP row
    key <AD01> { [      U0071,      U0051, VoidSymbol, VoidSymbol ] }; // q Q
    key <AD02> { [      U0077,      U0057, VoidSymbol,      U20A9 ] }; // w W L3 ‚Ç©
    key <AD03> { [      U0065,      U0045, VoidSymbol, VoidSymbol ] }; // e E
    key <AD04> { [      U0072,      U0052,      U00AE, VoidSymbol ] }; // r R ¬Æ
    key <AD05> { [      U0074,      U0054,      U2122, VoidSymbol ] }; // t T ‚Ñ¢
    key <AD06> { [      U0079,      U0059, VoidSymbol,      U00A5 ] }; // y Y L3 ¬•
    key <AD07> { [      U0075,      U0055, VoidSymbol, VoidSymbol ] }; // u U
    key <AD08> { [      U0069,      U0049,      U2018,      U201C ] }; // i I ‚Äò ‚Äú
    key <AD09> { [      U006F,      U004F,      U2019,      U201D ] }; // o O ‚Äô ‚Äù
    key <AD10> { [      U0070,      U0050, VoidSymbol,      U20B1 ] }; // p P L3 ‚Ç±
    key <AD11> { [      U005B,      U007B,      U2013,      U2012 ] }; // [ { ‚Äì ‚Äí (L3: en dash) (L4: figure dash)
    key <AD12> { [      U005D,      U007D,      U2014,      U2015 ] }; // ] } ‚Äî ‚Äï (L3: em dash) (L4: quotation/dialogue dash)
    key <BKSL> { [      U005C,      U007C,      U0259,      U018F ] }; // \ | …ô ∆è

    // HOME row
    key <AC01> { [      U0061,      U0041, VoidSymbol, VoidSymbol ] }; // a A
    key <AC02> { [      U0073,      U0053,     U1F6C8, VoidSymbol ] }; // s S üõà (L3: circled source mark)
    key <AC03> { [      U0064,      U0044, VoidSymbol, VoidSymbol ] }; // d D
    key <AC04> { [      U0066,      U0046, VoidSymbol, VoidSymbol ] }; // f F
    key <AC05> { [      U0067,      U0047, VoidSymbol, VoidSymbol ] }; // g G
    key <AC06> { [      U0068,      U0048, VoidSymbol, VoidSymbol ] }; // h H
    key <AC07> { [      U006A,      U004A, VoidSymbol, VoidSymbol ] }; // j J
    key <AC08> { [      U006B,      U004B, VoidSymbol, VoidSymbol ] }; // k K
    key <AC09> { [      U006C,      U004C, VoidSymbol, VoidSymbol ] }; // l L
    key <AC10> { [      U003B,      U003A,      U0304,      U035E ] }; // ; : ‚óåÃÑ ‚óåÕû (L3: combining macron | tuldik pahaba) (L4: combining double macron | tuldik pahaba)
    key <AC11> { [      U0027,      U0022,      U0301,      U0308 ] }; // ' " ‚óåÃÅ ‚óåÃà (L3: combining acute | tuldik pahilis) (L4: combining dieresis | tuldik patuldok)
    // AC12 is in some physical keyboards (e.g. OLPC); usually an alias of <BKSL>
    //key <AC12> { [   NoSymbol,   NoSymbol,   NoSymbol,   NoSymbol ] }; // (use what is in the fallback layout of the OS)

    // BOTTOM row
    // AB00 is in some physical keyboards (e.g. ISO-105)
    //key <AB00> { [   NoSymbol,   NoSymbol,   NoSymbol,   NoSymbol ] }; // (use what is in the fallback layout of the OS)
    key <AB01> { [      U007A,      U005A, VoidSymbol, VoidSymbol ] }; // z Z
    key <AB02> { [      U0078,      U0058, VoidSymbol, VoidSymbol ] }; // x X
    key <AB03> { [      U0063,      U0043,      U00A9, VoidSymbol ] }; // c C ¬©
    key <AB04> { [      U0076,      U0056, VoidSymbol, VoidSymbol ] }; // v V
    key <AB05> { [      U0062,      U0042, VoidSymbol, VoidSymbol ] }; // b B
    key <AB06> { [      U006E,      U004E,      U00F1,      U00D1 ] }; // n N √± √ë
    key <AB07> { [      U006D,      U004D, VoidSymbol, VoidSymbol ] }; // m M
    key <AB08> { [      U002C,      U003C, VoidSymbol,      U2423 ] }; // , < L3 ‚ê£ (L4: visible space mark)
    key <AB09> { [      U002E,      U003E,      U2026,      U00B7 ] }; // . > ‚Ä¶ ¬∑ (L3: ellipsis) (L4: middle dot | tuldok sa pagpapantig)
    key <AB10> { [      U002F,      U003F, VoidSymbol,      U00BF ] }; // / ? L3 ¬ø
    // AB11 is in some physical keyboards (e.g. Japanese)
    //key <AB11> { [   NoSymbol,   NoSymbol,   NoSymbol,   NoSymbol ] }; // (use what is in the fallback layout of the OS)

    // Modifiers
    include "level3(ralt_switch)" // RAlt/AltGr or <LVL3> key (see: /X11/xkb/symbols/level3)
  };
// END: Philippines Unicode Keyboard Layout - QWERTY

// BGN: Philippines Unicode Keyboard Layout - Colemak
  //
  // Code license: MIT
  // Layout license: Creative Commons Attribution (CC BY) 4.0 International
  //   * First released: 2010
  // Repo: https://codeberg.org/yelosan/unicode-keyboard-layout-philippines
  //
  // By: JC John Sese Cuneta (Yelosan Publishing)
  //   * Email: contact@im.youronly.one | jcjohn.sesecuneta@gmail.com
  // For: Ubuntu Philippines LoCo Team and the Filipino people
  //
  // Based on: Colemak as of 2023-02-05
  //   * https://colemak.com
  //
  partial alphanumeric_keys
  xkb_symbols "colemak" {
    name[Group1] = "Filipino (Colemak)";

    // NUMERIC row
    include "ph(numeric-row)"

    // TOP row
    key <AD01> { [      U0071,      U0051, VoidSymbol, VoidSymbol ] }; // q Q
    key <AD02> { [      U0077,      U0057, VoidSymbol,      U20A9 ] }; // w W L3 ‚Ç©
    key <AD03> { [      U0066,      U0046, VoidSymbol, VoidSymbol ] }; // f F
    key <AD04> { [      U0070,      U0050, VoidSymbol,      U20B1 ] }; // p P L3 ‚Ç±
    key <AD05> { [      U0067,      U0047, VoidSymbol, VoidSymbol ] }; // g G
    key <AD06> { [      U006A,      U004A, VoidSymbol, VoidSymbol ] }; // j J
    key <AD07> { [      U006C,      U004C, VoidSymbol, VoidSymbol ] }; // l L
    key <AD08> { [      U0075,      U0055, VoidSymbol, VoidSymbol ] }; // u U
    key <AD09> { [      U0079,      U0059, VoidSymbol,      U00A5 ] }; // y Y L3 ¬•
    key <AD10> { [      U003B,      U003A,      U0304,      U035E ] }; // ; : ‚óåÃÑ ‚óåÕû (L3: combining macron | tuldik pahaba) (L4: combining double macron | tuldik pahaba)
    key <AD11> { [      U005B,      U007B,      U2013,      U2012 ] }; // [ { ‚Äì ‚Äí (L3: en dash) (L4: figure dash)
    key <AD12> { [      U005D,      U007D,      U2014,      U2015 ] }; // ] } ‚Äî ‚Äï (L3: em dash) (L4: quotation/dialogue dash)
    key <BKSL> { [      U005C,      U007C,      U0259,      U018F ] }; // \ | …ô ∆è

    // HOME row
    key <AC01> { [      U0061,      U0041, VoidSymbol, VoidSymbol ] }; // a A
    key <AC02> { [      U0072,      U0052,      U00AE, VoidSymbol ] }; // r R ¬Æ
    key <AC03> { [      U0073,      U0053,     U1F6C8, VoidSymbol ] }; // s S üõà (L3: circled source mark)
    key <AC04> { [      U0074,      U0054,      U2122, VoidSymbol ] }; // t T ‚Ñ¢
    key <AC05> { [      U0064,      U0044, VoidSymbol, VoidSymbol ] }; // d D
    key <AC06> { [      U0068,      U0048, VoidSymbol, VoidSymbol ] }; // h H
    key <AC07> { [      U006E,      U004E,      U00F1,      U00D1 ] }; // n N √± √ë
    key <AC08> { [      U0065,      U0045, VoidSymbol, VoidSymbol ] }; // e E
    key <AC09> { [      U0069,      U0049,      U2018,      U201C ] }; // i I ‚Äò ‚Äú
    key <AC10> { [      U006F,      U004F,      U2019,      U201D ] }; // o O ‚Äô ‚Äù
    key <AC11> { [      U0027,      U0022,      U0301,      U0308 ] }; // ' " ‚óåÃÅ ‚óåÃà (L3: combining acute | tuldik pahilis) (L4: combining dieresis | tuldik patuldok)

    // BOTTOM row
    key <AB01> { [      U007A,      U005A, VoidSymbol, VoidSymbol ] }; // z Z
    key <AB02> { [      U0078,      U0058, VoidSymbol, VoidSymbol ] }; // x X
    key <AB03> { [      U0063,      U0043,      U00A9, VoidSymbol ] }; // c C ¬©
    key <AB04> { [      U0076,      U0056, VoidSymbol, VoidSymbol ] }; // v V
    key <AB05> { [      U0062,      U0042, VoidSymbol, VoidSymbol ] }; // b B
    key <AB06> { [      U006B,      U004B, VoidSymbol, VoidSymbol ] }; // k K
    key <AB07> { [      U006D,      U004D, VoidSymbol, VoidSymbol ] }; // m M
    key <AB08> { [      U002C,      U003C, VoidSymbol,      U2423 ] }; // , < L3 ‚ê£ (L4: visible space mark)
    key <AB09> { [      U002E,      U003E,      U2026,      U00B7 ] }; // . > ‚Ä¶ ¬∑ (L3: ellipsis) (L4: middle dot | tuldok sa pagpapantig)
    key <AB10> { [      U002F,      U003F, VoidSymbol,      U00BF ] }; // / ? L3 ¬ø

    // Modifiers
    include "level3(ralt_switch)" // RAlt/AltGr or <LVL3> key (see: /X11/xkb/symbols/level3)
  };
// END: Philippines Unicode Keyboard Layout - Colemak

// BGN: Philippines Unicode Keyboard Layout - Colemak-DH / Colemak-DHm
  //
  // Code license: MIT
  // Layout license: Creative Commons Attribution (CC BY) 4.0 International
  //   * First released: 2010
  // Repo: https://codeberg.org/yelosan/unicode-keyboard-layout-philippines
  //
  // By: JC John Sese Cuneta (Yelosan Publishing)
  //   * Email: contact@im.youronly.one | jcjohn.sesecuneta@gmail.com
  // For: Ubuntu Philippines LoCo Team and the Filipino people
  //
  // Based on: Colemak-DH as of 2023-02-05
  //   * https://colemakmods.github.io/mod-dh/
  //
  partial alphanumeric_keys
  xkb_symbols "colemak-dh" {
    name[Group1] = "Filipino (Colemak-DH)";

    include "ph(colemak)"

    // TOP row
    key <AD05> { [      U0062,      U0042, VoidSymbol, VoidSymbol ] }; // b B

    // HOME row
    key <AC05> { [      U0067,      U0047, VoidSymbol, VoidSymbol ] }; // g G
    key <AC06> { [      U006D,      U004D, VoidSymbol, VoidSymbol ] }; // m M

    // BOTTOM row
    key <AB01> { [      U0078,      U0058, VoidSymbol, VoidSymbol ] }; // x X
    key <AB02> { [      U0063,      U0043,      U00A9, VoidSymbol ] }; // c C ¬©
    key <AB03> { [      U0064,      U0044, VoidSymbol, VoidSymbol ] }; // d D
    key <AB05> { [      U007A,      U005A, VoidSymbol, VoidSymbol ] }; // z Z
    key <AB07> { [      U0068,      U0048, VoidSymbol, VoidSymbol ] }; // h H

    // Modifiers
    include "level3(ralt_switch)" // RAlt/AltGr or <LVL3> key (see: /X11/xkb/symbols/level3)
  };
// END: Philippines Unicode Keyboard Layout - Colemak-DH / Colemak-DHm

// BGN: Philippines Unicode Keyboard Layout - Capewell-QWERF2007
  //
  // Code license: MIT
  // Layout license: Creative Commons Attribution (CC BY) 4.0 International
  //   * First released: 2023
  // Repo: https://codeberg.org/yelosan/unicode-keyboard-layout-philippines
  //
  // By: JC John Sese Cuneta (Yelosan Publishing)
  //   * Email: contact@im.youronly.one | jcjohn.sesecuneta@gmail.com
  // For: Ubuntu Philippines LoCo Team and the Filipino people
  //
  // Based on: Capewell-QWERF2007 as of 2023-02-06
  //   * http://www.michaelcapewell.com/projects/keyboard/index.htm#The_QWERF_Layout
  //
  partial alphanumeric_keys
  xkb_symbols "capewell-qwerf2007" {
    name[Group1] = "Filipino (Capewell-QWERF2007)";

    // NUMERIC row
    include "ph(numeric-row)"

    // TOP row
    key <AD01> { [      U0071,      U0051, VoidSymbol, VoidSymbol ] }; // q Q
    key <AD02> { [      U0077,      U0057, VoidSymbol,      U20A9 ] }; // w W L3 ‚Ç©
    key <AD03> { [      U0065,      U0045, VoidSymbol, VoidSymbol ] }; // e E
    key <AD04> { [      U0072,      U0052,      U00AE, VoidSymbol ] }; // r R ¬Æ
    key <AD05> { [      U0066,      U0046, VoidSymbol, VoidSymbol ] }; // f F
    key <AD06> { [      U006A,      U004A, VoidSymbol, VoidSymbol ] }; // j J
    key <AD07> { [      U0079,      U0059, VoidSymbol,      U00A5 ] }; // y Y L3 ¬•
    key <AD08> { [      U006C,      U004C, VoidSymbol, VoidSymbol ] }; // l L
    key <AD09> { [      U006B,      U004B, VoidSymbol, VoidSymbol ] }; // k K
    key <AD10> { [      U003B,      U003A,      U0304,      U035E ] }; // ; : ‚óåÃÑ ‚óåÕû (L3: combining macron | tuldik pahaba) (L4: combining double macron | tuldik pahaba)
    key <AD11> { [      U005B,      U007B,      U2013,      U2012 ] }; // [ { ‚Äì ‚Äí (L3: en dash) (L4: figure dash)
    key <AD12> { [      U005D,      U007D,      U2014,      U2015 ] }; // ] } ‚Äî ‚Äï (L3: em dash) (L4: quotation/dialogue dash)
    key <BKSL> { [      U005C,      U007C,      U0259,      U018F ] }; // \ | …ô ∆è

    // HOME row
    key <AC01> { [      U0061,      U0041, VoidSymbol, VoidSymbol ] }; // a A
    key <AC02> { [      U0073,      U0053,     U1F6C8, VoidSymbol ] }; // s S üõà (L3: circled source mark)
    key <AC03> { [      U0064,      U0044, VoidSymbol, VoidSymbol ] }; // d D
    key <AC04> { [      U0074,      U0054,      U2122, VoidSymbol ] }; // t T ‚Ñ¢
    key <AC05> { [      U0067,      U0047, VoidSymbol, VoidSymbol ] }; // g G
    key <AC06> { [      U0068,      U0048, VoidSymbol, VoidSymbol ] }; // h H
    key <AC07> { [      U0075,      U0055, VoidSymbol, VoidSymbol ] }; // u U
    key <AC08> { [      U006F,      U004F,      U2018,      U201C ] }; // o O ‚Äò ‚Äú
    key <AC09> { [      U0069,      U0049,      U2019,      U201D ] }; // i I ‚Äô ‚Äù
    key <AC10> { [      U0070,      U0050, VoidSymbol,      U20B1 ] }; // p P L3 ‚Ç±
    key <AC11> { [      U0027,      U0022,      U0301,      U0308 ] }; // ' " ‚óåÃÅ ‚óåÃà (L3: combining acute | tuldik pahilis) (L4: combining dieresis | tuldik patuldok)

    // BOTTOM row
    key <AB01> { [      U007A,      U005A, VoidSymbol, VoidSymbol ] }; // z Z
    key <AB02> { [      U0078,      U0058, VoidSymbol, VoidSymbol ] }; // x X
    key <AB03> { [      U0063,      U0043,      U00A9, VoidSymbol ] }; // c C ¬©
    key <AB04> { [      U0076,      U0056, VoidSymbol, VoidSymbol ] }; // v V
    key <AB05> { [      U0062,      U0042, VoidSymbol, VoidSymbol ] }; // b B
    key <AB06> { [      U006E,      U004E,      U00F1,      U00D1 ] }; // n N √± √ë
    key <AB07> { [      U006D,      U004D, VoidSymbol, VoidSymbol ] }; // m M
    key <AB08> { [      U002C,      U003C, VoidSymbol,      U2423 ] }; // , < L3 ‚ê£ (L4: visible space mark)
    key <AB09> { [      U002E,      U003E,      U2026,      U00B7 ] }; // . > ‚Ä¶ ¬∑ (L3: ellipsis) (L4: middle dot | tuldok sa pagpapantig)
    key <AB10> { [      U002F,      U003F, VoidSymbol,      U00BF ] }; // / ? L3 ¬ø

    // Modifiers
    include "level3(ralt_switch)" // RAlt/AltGr or <LVL3> key (see: /X11/xkb/symbols/level3)
  };
// END: Philippines Unicode Keyboard Layout - Capewell-QWERF2007

// BGN: Philippines Unicode Keyboard Layout - Capewell-QWERF2006
  //
  // Code license: MIT
  // Layout license: Creative Commons Attribution (CC BY) 4.0 International
  //   * First released: 2010
  // Repo: https://codeberg.org/yelosan/unicode-keyboard-layout-philippines
  //
  // By: JC John Sese Cuneta (Yelosan Publishing)
  //   * Email: contact@im.youronly.one | jcjohn.sesecuneta@gmail.com
  // For: Ubuntu Philippines LoCo Team and the Filipino people
  //
  // Based on: Capewell-QWERF2006 as of 2023-02-06
  //   * http://www.michaelcapewell.com/projects/keyboard/index.htm#The_QWERF_Layout
  //
  partial alphanumeric_keys
  xkb_symbols "capewell-qwerf2006" {
    name[Group1] = "Filipino (Capewell-QWERF2006)";

    include "ph(capewell-qwerf2007)"

    // TOP row
    key <AD08> { [      U006B,      U004B, VoidSymbol, VoidSymbol ] }; // k K
    key <AD09> { [      U006C,      U004C, VoidSymbol, VoidSymbol ] }; // l L

    // HOME row
    key <AC08> { [      U0069,      U0049,      U2018,      U201C ] }; // i I ‚Äò ‚Äú
    key <AC09> { [      U006F,      U004F,      U2019,      U201D ] }; // o O ‚Äô ‚Äù

    // Modifiers
    include "level3(ralt_switch)" // RAlt/AltGr or <LVL3> key (see: /X11/xkb/symbols/level3)
  };
// END: Philippines Unicode Keyboard Layout - Capewell-QWERF2006

// BGN: Philippines Unicode Keyboard Layout - Dvorak
  //
  // Code license: MIT
  // Layout license: Creative Commons Attribution (CC BY) 4.0 International
  //   * First released: 2010
  // Repo: https://codeberg.org/yelosan/unicode-keyboard-layout-philippines
  //
  // By: JC John Sese Cuneta (Yelosan Publishing)
  //   * Email: contact@im.youronly.one | jcjohn.sesecuneta@gmail.com
  // For: Ubuntu Philippines LoCo Team and the Filipino people
  //
  // Based on: Dvorak US as of 2023-02-06
  //   * https://en.wikipedia.org/wiki/Dvorak_keyboard_layout
  //
  partial alphanumeric_keys
  xkb_symbols "dvorak" {
    name[Group1] = "Filipino (Dvorak)";

    // NUMERIC row
    include "ph(numeric-row)"

    // NUMERIC row
    key <AE11> { [      U005B,      U007B,      U2013,      U2012, VoidSymbol, VoidSymbol, VoidSymbol, VoidSymbol ] }; // [ { ‚Äì ‚Äí (L3: en dash) (L4: figure dash)
    key <AE12> { [      U005D,      U007D,      U2014,      U2015, VoidSymbol, VoidSymbol, VoidSymbol, VoidSymbol ] }; // ] } ‚Äî ‚Äï (L3: em dash) (L4: quotation/dialogue dash)

    // TOP row
    key <AD01> { [      U0027,      U0022,      U0301,      U0308 ] }; // ' " ‚óåÃÅ ‚óåÃà (L3: combining acute | tuldik pahilis) (L4: combining dieresis | tuldik patuldok)
    key <AD02> { [      U002C,      U003C, VoidSymbol,      U2423 ] }; // , < L3 ‚ê£ (L4: visible space mark)
    key <AD03> { [      U002E,      U003E,      U2026,      U00B7 ] }; // . > ‚Ä¶ ¬∑ (L3: ellipsis) (L4: middle dot | tuldok sa pagpapantig)
    key <AD04> { [      U0070,      U0050, VoidSymbol,      U20B1 ] }; // p P L3 ‚Ç±
    key <AD05> { [      U0079,      U0059, VoidSymbol,      U00A5 ] }; // y Y L3 ¬•
    key <AD06> { [      U0066,      U0046, VoidSymbol, VoidSymbol ] }; // f F
    key <AD07> { [      U0067,      U0047, VoidSymbol, VoidSymbol ] }; // g G
    key <AD08> { [      U0063,      U0043,      U00A9, VoidSymbol ] }; // c C ¬©
    key <AD09> { [      U0072,      U0052,      U00AE, VoidSymbol ] }; // r R ¬Æ
    key <AD10> { [      U006C,      U004C, VoidSymbol, VoidSymbol ] }; // l L
    key <AD11> { [      U002F,      U003F, VoidSymbol,      U00BF ] }; // / ? L3 ¬ø
    key <AD12> { [      U003D,      U002B,      U00F7,      U00D7 ] }; // = + √∑ √ó
    key <BKSL> { [      U005C,      U007C,      U0259,      U018F ] }; // \ | …ô ∆è

    // HOME row
    key <AC01> { [      U0061,      U0041,      U2018,      U201C ] }; // a A ‚Äò ‚Äú
    key <AC02> { [      U006F,      U004F,      U2019,      U201D ] }; // o O ‚Äô ‚Äù
    key <AC03> { [      U0065,      U0045, VoidSymbol, VoidSymbol ] }; // e E
    key <AC04> { [      U0075,      U0055, VoidSymbol, VoidSymbol ] }; // u U
    key <AC05> { [      U0069,      U0049, VoidSymbol, VoidSymbol ] }; // i I
    key <AC06> { [      U0064,      U0044, VoidSymbol, VoidSymbol ] }; // d D
    key <AC07> { [      U0068,      U0048, VoidSymbol, VoidSymbol ] }; // h H
    key <AC08> { [      U0074,      U0054,      U2122, VoidSymbol ] }; // t T ‚Ñ¢
    key <AC09> { [      U006E,      U004E,      U00F1,      U00D1 ] }; // n N √± √ë
    key <AC10> { [      U0073,      U0053,     U1F6C8, VoidSymbol ] }; // s S üõà (L3: circled source mark)
    key <AC11> { [      U002D,      U005F, VoidSymbol, VoidSymbol ] }; // - _

    // BOTTOM row
    key <AB01> { [      U003B,      U003A,      U0304,      U035E ] }; // ; : ‚óåÃÑ ‚óåÕû (L3: combining macron | tuldik pahaba) (L4: combining double macron | tuldik pahaba)
    key <AB02> { [      U0071,      U0051, VoidSymbol, VoidSymbol ] }; // q Q
    key <AB03> { [      U006A,      U004A, VoidSymbol, VoidSymbol ] }; // j J
    key <AB04> { [      U006B,      U004B, VoidSymbol, VoidSymbol ] }; // k K
    key <AB05> { [      U0078,      U0058, VoidSymbol, VoidSymbol ] }; // x X
    key <AB06> { [      U0062,      U0042, VoidSymbol, VoidSymbol ] }; // b B
    key <AB07> { [      U006D,      U004D, VoidSymbol, VoidSymbol ] }; // m M
    key <AB08> { [      U0077,      U0057, VoidSymbol,      U20A9 ] }; // w W L3 ‚Ç©
    key <AB09> { [      U0076,      U0056, VoidSymbol, VoidSymbol ] }; // v V
    key <AB10> { [      U007A,      U005A, VoidSymbol, VoidSymbol ] }; // z Z

    // Modifiers
    include "level3(ralt_switch)" // RAlt/AltGr or <LVL3> key (see: /X11/xkb/symbols/level3)
  };
// END: Philippines Unicode Keyboard Layout - Dvorak

