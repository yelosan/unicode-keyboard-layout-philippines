    key <BKSL> { [ VoidSymbol, VoidSymbol ] }; // (\)

    // HOME row
    key <AC01> { [      U1700, VoidSymbol ] }; // (A) ·úÄ (LETTER A)
    key <AC02> { [      U1710, VoidSymbol ] }; // (S) ·úê (LETTER SA)
    key <AC03> { [      U1707, VoidSymbol ] }; // (D) ·úá (LETTER DA)
    key <AC04> { [ VoidSymbol, VoidSymbol ] }; // (F)
    key <AC05> { [      U1704, VoidSymbol ] }; // (G) ·úÑ (LETTER GA)
    key <AC06> { [      U1711, VoidSymbol ] }; // (H) ·úë (LETTER HA)
    key <AC07> { [ VoidSymbol, VoidSymbol ] }; // (J)
    key <AC08> { [      U1703, VoidSymbol ] }; // (K) ·úÉ (LETTER KA)
    key <AC09> { [      U170E, VoidSymbol ] }; // (L) ·úé (LETTER LA)
    key <AC10> { [ VoidSymbol, VoidSymbol ] }; // (;)
    key <AC11> { [ VoidSymbol, VoidSymbol ] }; // (')

    // BOTTOM row
    key <AB01> { [ VoidSymbol, VoidSymbol ] }; // (Z)
    key <AB02> { [ VoidSymbol, VoidSymbol ] }; // (X)
