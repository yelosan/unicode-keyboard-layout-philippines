    key <AC04> { [      U1706, VoidSymbol ] }; // (T) ·úÜ (LETTER TA)
    key <AC05> { [      U1707, VoidSymbol ] }; // (D) ·úá (LETTER DA)
    key <AC06> { [      U1711, VoidSymbol ] }; // (H) ·úë (LETTER HA)
    key <AC07> { [      U1708,      U1705 ] }; // (N) ·úà (LETTER NA) ·úÖ (LETTER NGA)
    key <AC08> { [      U1712,      U1701 ] }; // (E) ·úí (VOWEL E) ·úÅ (LETTER E)
    key <AC09> { [      U1712,      U1701 ] }; // (I) ·úí (VOWEL I) ·úÅ (LETTER I)
    key <AC10> { [      U1713,      U1702 ] }; // (O) ·úì (VOWEL O) ·úÇ (LETTER O)
    key <AC11> { [ VoidSymbol, VoidSymbol ] }; // (')

    // BOTTOM row
