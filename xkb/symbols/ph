    key <AD03> { [      U1712,      U1701 ] }; // (E) ·úí (VOWEL E) ·úÅ (LETTER E)
    key <AD04> { [      U170D,      U171F ] }; // (R) ·úç (LETTER RA) ·úü (LETTER ARCHAIC RA)
