  //
  // Code license: MIT
  // Layout license: Creative Commons Attribution (CC BY) 4.0 International
  //   * First released: 2010
  // Repo: https://codeberg.org/yelosan/unicode-keyboard-layout-philippines
  //
  // By: JC John Sese Cuneta (Yelosan Publishing)
  //   * Email: contact@im.youronly.one | jcjohn.sesecuneta@gmail.com
  // For: Ubuntu Philippines LoCo Team and the Filipino people
  //
  // Based on: Capewell-Dvorak as of 2023-02-06
  //   * http://www.michaelcapewell.com/projects/keyboard/index.htm#The_Capewell-Dvorak_Layout
  //
  partial alphanumeric_keys
  xkb_symbols "capewell-dvorak" {
    name[Group1] = "Filipino (Capewell-Dvorak)";

    include "ph(dvorak)"

    // TOP row
    key <AD06> { [      U0071,      U0051, VoidSymbol, VoidSymbol ] }; // q Q
    key <AD07> { [      U0066,      U0046, VoidSymbol, VoidSymbol ] }; // f F
    key <AD08> { [      U0067,      U0047, VoidSymbol, VoidSymbol ] }; // g G
    key <AD10> { [      U006B,      U004B, VoidSymbol, VoidSymbol ] }; // k K

    // HOME row
    key <AC01> { [      U006F,      U004F,      U2018,      U201C ] }; // o O ‚Äò ‚Äú
    key <AC02> { [      U0061,      U0041,      U2019,      U201D ] }; // a A ‚Äô ‚Äù
    key <AC04> { [      U0069,      U0049, VoidSymbol, VoidSymbol ] }; // i I
    key <AC05> { [      U0075,      U0055, VoidSymbol, VoidSymbol ] }; // u U

    // BOTTOM row
    key <AB01> { [      U007A,      U005A, VoidSymbol, VoidSymbol ] }; // z Z
    key <AB02> { [      U0078,      U0058, VoidSymbol, VoidSymbol ] }; // x X
    key <AB03> { [      U0063,      U0043,      U00A9, VoidSymbol ] }; // c C ¬©
    key <AB04> { [      U0076,      U0056, VoidSymbol, VoidSymbol ] }; // v V
    key <AB05> { [      U006A,      U004A, VoidSymbol, VoidSymbol ] }; // j J
    key <AB06> { [      U006C,      U004C, VoidSymbol, VoidSymbol ] }; // l L
    key <AB09> { [      U0062,      U0042, VoidSymbol, VoidSymbol ] }; // b B
    key <AB10> { [      U003B,      U003A,      U0304,      U035E ] }; // ; : ‚óåÃÑ ‚óåÕû (L3: combining macron | tuldik pahaba) (L4: combining double macron | tuldik pahaba)

    // Modifiers
    include "level3(ralt_switch)" // RAlt/AltGr or <LVL3> key (see: /X11/xkb/symbols/level3)
  };
// END: Philippines Unicode Keyboard Layout - Capewell-Dvorak / C-Dvorak


// BGN: Baybayin-Buhid Unicode Keyboard Layout - QWERTY
  // ISO tag for Baybayin-Buhid script:
  //   * Buhd (U+1740‚ÄìU+175F) - https://www.unicode.org/charts/PDF/U1740.pdf)
  //
  // Code license: MIT No Attribution (MIT-0)
  // Layout license: CC0 1.0 Universal (Public Domain)
  //   * First released: 2023
  // Repo: https://codeberg.org/yelosan/unicode-keyboard-layout-philippines
  //
  // By: JC John Sese Cuneta (Yelosan Publishing)
  //   * Email: contact@im.youronly.one | jcjohn.sesecuneta@gmail.com
  // For: Ubuntu Philippines LoCo Team and the Filipino people
  //
  partial alphanumeric_keys
  xkb_symbols "buhd-qwerty" {
    name[Group1] = "Buhid (Baybayin)";
    key.type[Group1] = "TWO_LEVEL";

    // NUMERIC row
    include "ph(baybayin-numeric-row)"

    // TOP row
    key <AD01> { [ VoidSymbol, VoidSymbol ] }; // (Q)
    key <AD02> { [      U174F, VoidSymbol ] }; // (W) ·ùè (LETTER WA)
    key <AD03> { [      U1752,      U1741 ] }; // (E) ·ùí (VOWEL E) ·ùÅ (LETTER E)
    key <AD04> { [      U174D, VoidSymbol ] }; // (R) ·ùç (LETTER RA)
    key <AD05> { [      U1746, VoidSymbol ] }; // (T) ·ùÜ (LETTER TA)
    key <AD06> { [      U174C, VoidSymbol ] }; // (Y) ·ùå (LETTER YA)
    key <AD07> { [      U1753,      U1742 ] }; // (U) ·ùì(VOWEL U) ·ùÇ (LETTER U)
    key <AD08> { [      U1752,      U1741 ] }; // (I) ·ùí (VOWEL I) ·ùÅ (LETTER I)
    key <AD09> { [      U1753,      U1742 ] }; // (O) ·ùì (VOWEL O) ·ùÇ (LETTER O)
    key <AD10> { [      U1749, VoidSymbol ] }; // (P) ·ùâ (LETTER PA)
    key <AD11> { [ VoidSymbol, VoidSymbol ] }; // ([)
    key <AD12> { [ VoidSymbol, VoidSymbol ] }; // (])
    key <BKSL> { [ VoidSymbol, VoidSymbol ] }; // (\)

    // HOME row
    key <AC01> { [      U1740, VoidSymbol ] }; // (A) ·ùÄ (LETTER A)
    key <AC02> { [      U1750, VoidSymbol ] }; // (S) ·ùê (LETTER SA)
    key <AC03> { [      U1747, VoidSymbol ] }; // (D) ·ùá (LETTER DA)
    key <AC04> { [ VoidSymbol, VoidSymbol ] }; // (F)
    key <AC05> { [      U1744, VoidSymbol ] }; // (G) ·ùÑ (LETTER GA)
    key <AC06> { [      U1751, VoidSymbol ] }; // (H) ·ùë (LETTER HA)
    key <AC07> { [ VoidSymbol, VoidSymbol ] }; // (J)
    key <AC08> { [      U1743, VoidSymbol ] }; // (K) ·ùÉ (LETTER KA)
    key <AC09> { [      U174E, VoidSymbol ] }; // (L) ·ùé (LETTER LA)
    key <AC10> { [ VoidSymbol, VoidSymbol ] }; // (;)
    key <AC11> { [ VoidSymbol, VoidSymbol ] }; // (')

    // BOTTOM row
    key <AB01> { [ VoidSymbol, VoidSymbol ] }; // (Z)
    key <AB02> { [ VoidSymbol, VoidSymbol ] }; // (X)
    key <AB03> { [ VoidSymbol, VoidSymbol ] }; // (C)
    key <AB04> { [ VoidSymbol, VoidSymbol ] }; // (V)
    key <AB05> { [      U174A, VoidSymbol ] }; // (B) ·ùä (LETTER BA)
    key <AB06> { [      U1748,      U1745 ] }; // (N) ·ùà (LETTER NA) ·ùÖ (LETTER NGA)
    key <AB07> { [      U174B, VoidSymbol ] }; // (M) ·ùã (LETTER MA)

    include "ph(mangyan-punctuations)"
  };
// END: Baybayin-Buhid Unicode Keyboard Layout - QWERTY

