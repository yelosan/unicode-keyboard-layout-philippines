  partial alphanumeric_keys
  xkb_symbols "tagb-colemak-dh" {
    name[Group1] = "Tagbanwa (Baybayin, Colemak-DH)";
    key.type[Group1] = "TWO_LEVEL";

    include "ph(tagb-colemak)"

    // TOP row
    key <AD05> { [      U176A, VoidSymbol ] }; // (B) ·ù™ (LETTER BA)

    // HOME row
    key <AC05> { [      U1764, VoidSymbol ] }; // (G) ·ù§ (LETTER GA)
    key <AC06> { [      U176B, VoidSymbol ] }; // (M) ·ù´ (LETTER MA)

    // BOTTOM row
    key <AB03> { [      U1767, VoidSymbol ] }; // (D) ·ùß (LETTER DA)
    key <AB05> { [ VoidSymbol, VoidSymbol ] }; // (Z) override 'include'
    key <AB07> { [ VoidSymbol, VoidSymbol ] }; // (H) override 'include'
  };
// END: Baybayin-Tagbanwa Unicode Keyboard Layout - Colemak-DH / Colemak-DHm

