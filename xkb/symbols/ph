    key <AC03> { [      U1712,      U1701 ] }; // (E) ·úí (VOWEL E) ·úÅ (LETTER E)
    key <AC04> { [      U1713,      U1702 ] }; // (U) ·úì (VOWEL U) ·úÇ (LETTER U)
