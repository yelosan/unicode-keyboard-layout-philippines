    key <AB05> { [      U170A, VoidSymbol ] }; // (B) ·úä (LETTER BA)
    key <AB06> { [      U1708,      U1705 ] }; // (N) ·úà (LETTER NA) ·úÖ (LETTER NGA)
    key <AB07> { [      U170B, VoidSymbol ] }; // (M) ·úã (LETTER MA)

    include "ph(baybayin-punctuations)"
  };
