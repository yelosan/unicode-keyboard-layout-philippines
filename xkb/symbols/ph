    key <AE11> { [ VoidSymbol, VoidSymbol ] };
    key <AE12> { [ VoidSymbol, VoidSymbol ] };

    // TOP row
    key <AD01> { [ VoidSymbol, VoidSymbol ] }; // (')
    key <AD02> { [      U1735, VoidSymbol ] }; // (,) ·úµ (PHILIPPINE SINGLE PUNCTUATION)
    key <AD03> { [      U1736, VoidSymbol ] }; // (.) ·ú∂ (PHILIPPINE DOUBLE PUNCTUATION)
    key <AD04> { [      U1729, VoidSymbol ] }; // (P) ·ú© (LETTER PA)
    key <AD05> { [      U172C, VoidSymbol ] }; // (Y) ·ú¨ (LETTER YA)
    key <AD06> { [ VoidSymbol, VoidSymbol ] }; // (F)
    key <AD07> { [      U1724, VoidSymbol ] }; // (G) ·ú§ (LETTER GA)
    key <AD08> { [ VoidSymbol, VoidSymbol ] }; // (C)
    key <AD09> { [      U172D, VoidSymbol ] }; // (R) ·ú≠ (LETTER RA)
    key <AD10> { [      U172E, VoidSymbol ] }; // (L) ·úÆ (LETTER LA)
    key <AD11> { [      U1734, VoidSymbol ] }; // (/) ·ú¥ (MANGYAN PAMUDPOD)
    key <AD12> { [      U1714, VoidSymbol ] }; // (+) ·úî (VIRAMA)
    key <BKSL> { [ VoidSymbol, VoidSymbol ] }; // (\)

    // HOME row
    key <AC01> { [      U1720, VoidSymbol ] }; // (A) ·ú† (LETTER A)
    key <AC02> { [      U1733,      U1722 ] }; // (O) ·ú≥ (VOWEL O) ·ú¢ (LETTER O)
    key <AC03> { [      U1732,      U1721 ] }; // (E) ·ú≤(VOWEL E) ·ú° (LETTER E)
    key <AC04> { [      U1733,      U1722 ] }; // (U) ·ú≥ (VOWEL U) ·ú¢ (LETTER U)
    key <AC05> { [      U1732,      U1721 ] }; // (I) ·ú≤ (VOWEL I) ·ú° (LETTER I)
    key <AC06> { [      U1727, VoidSymbol ] }; // (D) ·úß (LETTER DA)
    key <AC07> { [      U1731, VoidSymbol ] }; // (H) ·ú± (LETTER HA)
    key <AC08> { [      U1726, VoidSymbol ] }; // (T) ·ú¶ (LETTER TA)
    key <AC09> { [      U1728,      U1725 ] }; // (N) ·ú® (LETTER NA) ·ú• (LETTER NGA)
    key <AC10> { [      U1730, VoidSymbol ] }; // (S) ·ú∞ (LETTER SA)
    key <AC11> { [      U1734, VoidSymbol ] }; // (-) ·ú¥ (MANGYAN PAMUDPOD)

    // BOTTOM row
    key <AB01> { [ VoidSymbol, VoidSymbol ] }; // (;)
    key <AB02> { [ VoidSymbol, VoidSymbol ] }; // (Q)
    key <AB03> { [ VoidSymbol, VoidSymbol ] }; // (J)
    key <AB04> { [      U1723, VoidSymbol ] }; // (K) ·ú£ (LETTER KA)
