    key <AE11> { [ VoidSymbol, VoidSymbol ] };
    key <AE12> { [ VoidSymbol, VoidSymbol ] };

    // TOP row
    key <AD01> { [ VoidSymbol, VoidSymbol ] }; // (')
    key <AD02> { [      U1735, VoidSymbol ] }; // (,) ·úµ (PHILIPPINE SINGLE PUNCTUATION)
    key <AD03> { [      U1736, VoidSymbol ] }; // (.) ·ú∂ (PHILIPPINE DOUBLE PUNCTUATION)
    key <AD04> { [      U1749, VoidSymbol ] }; // (P) ·ùâ (LETTER PA)
    key <AD05> { [      U174C, VoidSymbol ] }; // (Y) ·ùå (LETTER YA)
    key <AD06> { [ VoidSymbol, VoidSymbol ] }; // (F)
    key <AD07> { [      U1744, VoidSymbol ] }; // (G) ·ùÑ (LETTER GA)
    key <AD08> { [ VoidSymbol, VoidSymbol ] }; // (C)
    key <AD09> { [      U174D, VoidSymbol ] }; // (R) ·ùç (LETTER RA)
    key <AD10> { [      U174E, VoidSymbol ] }; // (L) ·ùé (LETTER LA)
    key <AD11> { [      U1734, VoidSymbol ] }; // (/) ·ú¥ (MANGYAN PAMUDPOD)
    key <AD12> { [      U1714, VoidSymbol ] }; // (+) ·úî (VIRAMA)
    key <BKSL> { [ VoidSymbol, VoidSymbol ] }; // (\)

    // HOME row
    key <AC01> { [      U1740, VoidSymbol ] }; // (A) ·ùÄ (LETTER A)
    key <AC02> { [      U1753,      U1742 ] }; // (O) ·ùì (VOWEL O) ·ùÇ (LETTER O)
    key <AC03> { [      U1752,      U1741 ] }; // (E) ·ùí (VOWEL E) ·ùÅ (LETTER E)
    key <AC04> { [      U1753,      U1742 ] }; // (U) ·ùì(VOWEL U) ·ùÇ (LETTER U)
    key <AC05> { [      U1752,      U1741 ] }; // (I) ·ùí (VOWEL I) ·ùÅ (LETTER I)
    key <AC06> { [      U1747, VoidSymbol ] }; // (D) ·ùá (LETTER DA)
    key <AC07> { [      U1751, VoidSymbol ] }; // (H) ·ùë (LETTER HA)
    key <AC08> { [      U1746, VoidSymbol ] }; // (T) ·ùÜ (LETTER TA)
    key <AC09> { [      U1748,      U1745 ] }; // (N) ·ùà (LETTER NA) ·ùÖ (LETTER NGA)
    key <AC10> { [      U1750, VoidSymbol ] }; // (S) ·ùê (LETTER SA)
    key <AC11> { [      U1734, VoidSymbol ] }; // (-) ·ú¥ (MANGYAN PAMUDPOD)

    // BOTTOM row
    key <AB01> { [ VoidSymbol, VoidSymbol ] }; // (;)
    key <AB02> { [ VoidSymbol, VoidSymbol ] }; // (Q)
    key <AB03> { [ VoidSymbol, VoidSymbol ] }; // (J)
    key <AB04> { [      U1743, VoidSymbol ] }; // (K) ·ùÉ (LETTER KA)
    key <AB05> { [ VoidSymbol, VoidSymbol ] }; // (X)
    key <AB06> { [      U174A, VoidSymbol ] }; // (B) ·ùä (LETTER BA)
    key <AB07> { [      U174B, VoidSymbol ] }; // (M) ·ùã (LETTER MA)
    key <AB08> { [      U174F, VoidSymbol ] }; // (W) ·ùè (LETTER WA)
    key <AB09> { [ VoidSymbol, VoidSymbol ] }; // (V)
    key <AB10> { [ VoidSymbol, VoidSymbol ] }; // (Z)
  };
