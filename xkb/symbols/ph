  xkb_symbols "tagb-capewell-dvorak" {
    name[Group1] = "Tagbanwa (Baybayin, Capewell-Dvorak)";
    key.type[Group1] = "TWO_LEVEL";

    include "ph(tagb-dvorak)"

    // TOP row
    key <AD07> { [ VoidSymbol, VoidSymbol ] }; // (F) override 'include'
    key <AD08> { [      U1764, VoidSymbol ] }; // (G) ·ù§ (LETTER GA)
    key <AD10> { [      U1763, VoidSymbol ] }; // (K) ·ù£ (LETTER KA)

    // HOME row
    key <AC01> { [      U1773,      U1762 ] }; // (O) ‚óå·ù≥ (VOWEL O) ·ù¢ (LETTER O)
    key <AC02> { [      U1760, VoidSymbol ] }; // (A) ·ù† (LETTER A)
    key <AC04> { [      U1772,      U1761 ] }; // (I) ‚óå·ù≤ (VOWEL I) ·ù° (LETTER I)
    key <AC05> { [      U1773,      U1762 ] }; // (U) ‚óå·ù≥ (VOWEL U) ·ù¢ (LETTER U)

    // BOTTOM row
    key <AB04> { [ VoidSymbol, VoidSymbol ] }; // (V) override 'include'
    key <AB06> { [      U176E, VoidSymbol ] }; // (L) ·ùÆ (LETTER LA)
    key <AB09> { [      U176A, VoidSymbol ] }; // (B) ·ù™ (LETTER BA)
  };
// END: Baybayin-Tagbanwa Unicode Keyboard Layout - Capewell-Dvorak / C-Dvorak


// BGN: FILIPINO NUMERIC row
  //
  partial alphanumeric_keys
  xkb_symbols "numeric-row" {

    // NUMERIC row
    // TLDE is AE00 in some physical keyboards (e.g. OLPC)
    key <TLDE> { [      U0060,      U007E,      U0300,      U0303 ] }; // ` ~ ‚óåÃÄ ‚óåÃÉ (L3: combining grave | tuldik paiwa) (L4: combining tilde)
    key <AE01> { [      U0031,      U0021,      U0360,      U00A1 ] }; // 1 ! ‚óåÕ† ¬° (L3: combining double tilde) (L4: inverted exclamation point)
    key <AE02> { [      U0032,      U0040,      U2153, VoidSymbol ] }; // 2 @ ‚Öì
    key <AE03> { [      U0033,      U0023,      U2154,      U00A3 ] }; // 3 # ‚Öî ¬£
    key <AE04> { [      U0034,      U0024,      U00BC,      U20AC ] }; // 4 $ ¬º ‚Ç¨
    key <AE05> { [      U0035,      U0025,      U00BD, VoidSymbol ] }; // 5 % ¬Ω
    key <AE06> { [      U0036,      U005E,      U00BE,      U0302 ] }; // 6 ^ ¬æ ‚óåÃÇ (L4: combining circumflex | tuldik pakupya)
    key <AE07> { [      U0037,      U0026, VoidSymbol,      U2103 ] }; // 7 & L3 ‚ÑÉ
    key <AE08> { [      U0038,      U002A, VoidSymbol,      U00B0 ] }; // 8 * L3 ¬∞ (L4: degree symbol)
    key <AE09> { [      U0039,      U0028,      U2039,      U00AB ] }; // 9 ( ‚Äπ ¬´ (L3: Asian-Latin single open quote mark) (L4: Asian-Latin double open quote mark)
    key <AE10> { [      U0030,      U0029,      U203A,      U00BB ] }; // 0 ) ‚Ä∫ ¬ª (L3: Asian-Latin single close quote mark) (L4: Asian Latin double close quote mark)
    key <AE11> { [      U002D,      U005F, VoidSymbol, VoidSymbol ] }; // - _
    key <AE12> { [      U003D,      U002B,      U00F7,      U00D7 ] }; // = + √∑ √ó
    // AE13 is in some physical keyboards (e.g. Japanese)
    key <AE13> { [      U00A5,      U007C,      U20B1, VoidSymbol ] }; // ¬• | ‚Ç±
  };
// END: FILIPINO NUMERIC row

