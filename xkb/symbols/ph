    key <AC05> { [      U1764, VoidSymbol ] }; // (G) ·ù§ (LETTER GA)
    key <AC06> { [ VoidSymbol, VoidSymbol ] }; // (H)
    key <AC07> { [      U1773,      U1762 ] }; // (U) ‚óå·ù≥ (VOWEL U) ·ù¢ (LETTER U)
    key <AC08> { [      U1773,      U1762 ] }; // (O) ‚óå·ù≥ (VOWEL O) ·ù¢ (LETTER O)
