// BGN: BAYBAYIN PUNCTUATIONS
  //
  partial alphanumeric_keys
  xkb_symbols "baybayin-punctuations" {
    key.type[Group1] = "TWO_LEVEL";

    // NUMERIC row
    key <AE11> { [      U1715, VoidSymbol ] }; // (-) ·úï (TGLG PAMUDPOD)
    key <AE12> { [      U1714, VoidSymbol ] }; // (+) ·úî (VIRAMA)

    // BOTTOM row
    key <AB08> { [      U1735, VoidSymbol ] }; // (,) ·úµ (PHILIPPINE SINGLE PUNCTUATION)
    key <AB09> { [      U1736, VoidSymbol ] }; // (.) ·ú∂ (PHILIPPINE DOUBLE PUNCTUATION)
    key <AB10> { [      U1715, VoidSymbol ] }; // (/) ·úï (TGLG PAMUDPOD)
  };
// END: BAYBAYIN PUNCTUATIONS

